able {","        case food","        case music","        case gardening","        case sport","    }","","    func topicsChipsValues() -> [OUDSChipPickerData<Topics>] {","        [","            OUDSChipPickerData(tag: Topics.food, layout: .text(text: \"Food\")),","            OUDSChipPickerData(tag: Topics.music, layout: .text(text: \"Music\")),","            OUDSChipPickerData(tag: Topics.gardening, layout: .text(text: \"Gardening\")),","            OUDSChipPickerData(tag: Topics.sport, layout: .text(text: \"Sport\")),","        ]","    }","","    enum Genders: String, Hashable {","        case male","        case female","        case nonBinary","    }","","    func genderRadioValues() -> [OUDSRadioPickerData<Genders>] {","        [","            OUDSRadioPickerData(tag: Genders.male, label: \"Mr\"),","            OUDSRadioPickerData(tag: Genders.female, label: \"Ms\"),","            OUDSRadioPickerData(tag: Genders.nonBinary, label: \"Other\"),","        ]","    }","}"],"fileName":"Content.swift","highlights":[{"line":23},{"line":39},{"line":40},{"line":66},{"line":67},{"line":68},{"line":69},{"line":70},{"line":71},{"line":72},{"line":73},{"line":74},{"line":75},{"line":76},{"line":77},{"line":78},{"line":79},{"line":80},{"line":81}],"identifier":"tutorial-4-2.swift","type":"file"},"doc://OUDSThemesContract/tutorials/Table-Of-Contents/Richer-and-more-original-components-of-OUDS":{"title":"Richer and more original components of OUDS","kind":"article","url":"\/tutorials\/table-of-contents\/richer-and-more-original-components-of-ouds","identifier":"doc:\/\/OUDSThemesContract\/tutorials\/Table-Of-Contents\/Richer-and-more-original-components-of-OUDS","type":"topic","role":"article","abstract":[]},"doc://OUDSThemesContract/tutorials/OUDSThemesContract/tutorial-5-styling-things#Want-to-be-an-artist":{"abstract":[{"text":"Even if components increase the identity of your apps, it is not enough. Typography, fonts and tokens are here to improve that.","type":"text"}],"t