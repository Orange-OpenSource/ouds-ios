epted: Bool = false","    @State var switchToDarkMode: Bool = false","    @State var creatingProfile: Bool = false","","    @Environment(\\.openURL) var openUrl","","    var body: some View {","        VStack {","            HStack {","                OUDSTag(label: \"Welcome\")","                OUDSTag(label: \"OUDS\")","                OUDSTag(label: \"Tutorial\")","            }","","            OUDSRadioPicker(selection: $selectedGender,","                            radios: genderRadioValues(),","                            placement: .horizontal(false))","","            OUDSHorizontalDivider(color: .brandPrimary)","","            OUDSCheckboxItem(isOn: $termsAccepted, label: \"I accept the terms of use\")","","            OUDSSwitchItem(\"Switch to dark mode\", isOn: $switchToDarkMode)","","            OUDSLink(text: \"Go to the website\", indicator: .next) {","                openUrl.callAsFunction(URL(string: \"https:\/\/ios.unified-design-system.orange.com\")!)","            }","","            OUDSButton(text: \"Create my profile\",","                       appearance: .strong,","                       style: creatingProfile ? .loading : .default)","            {","                creatingProfile = true","                DispatchQueue.main.asyncAfter(deadline: .now() + 5) {","                    creatingProfile = false","                }","            }.disabled(!termsAccepted)","","        }.preferredColorScheme(switchToDarkMode ? .dark : .light)","    }","","    \/\/ MARK: - Utils","","    enum Genders: String, Hashable {","        case male","        case female","        case nonBinary","    }","","    func genderRadioValues() -> [OUDSRadioPickerData<Genders>] {","        [","            OUDSRadioPickerData(tag: Genders.male, label: \"Mr\"),","            OUDSRadioPickerData(tag: Genders.female, label: \"Ms\"),","            OUDSRadioPickerData(tag: Genders.nonBinary, label: \"Other\"),","        ]","    }","}"],"fileName":"Content.swift","highlights"