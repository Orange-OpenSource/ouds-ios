tle":[{"text":"OUDSRadio","kind":"identifier"}],"url":"\/documentation\/oudscomponents\/oudsradio","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OUDSRadio","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSRadio","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSRadio","isActive":true,"type":"reference"},{"type":"text","text":" proposes layout to add in your views a lonely radio, without labels, texts nor icons radios components"},{"type":"text","text":" "},{"type":"text","text":"If you want to use a radio with additional texts and icon, prefer instead "},{"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSRadioItem","isActive":true,"type":"reference"},{"type":"text","text":"."}],"kind":"symbol","role":"symbol"},"doc://OUDSComponents/documentation/OUDSComponents/Foundation":{"title":"Foundation","url":"\/documentation\/oudscomponents\/foundation","identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/Foundation","role":"collection","kind":"symbol","type":"topic","abstract":[]},"doc://OUDSComponents/documentation/OUDSComponents/AccessibilityFocusable":{"url":"\/documentation\/oudscomponents\/accessibilityfocusable","abstract":[{"type":"text","text":"Value to use as a "},{"code":"AccessibilityFocusState","type":"codeVoice"},{"type":"text","text":" to request focus."}],"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/AccessibilityFocusable","kind":"symbol","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"AccessibilityFocusable"}],"type":"topic","navigatorTitle":[{"kind":"identifier","text":"AccessibilityFocusable"}],"role":"symbol","title":"AccessibilityFocusable"},"doc://OUDSComponents/documentation/OUDSComponents/SwiftUICore":{"role":"collection","abstract":[],"kind":"symbol","identifier":"doc:\/\/O