r components containing icons like "},{"type":"codeVoice","code":"OUDSTag"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"link"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"For links like "},{"code":"OUDSLink","type":"codeVoice"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"pinCodeInput"}],"type":"paragraph"}],[{"inlineContent":[],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"quantityInput"}],"type":"paragraph"}],[{"inlineContent":[],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"radioButton"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"For radio components like "},{"code":"OUDSRadio","type":"codeVoice"},{"type":"text","text":" and "},{"code":"OUDSRadioItem","type":"codeVoice"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"selectInput"}],"type":"paragraph"}],[{"inlineContent":[],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"skeleton"}],"type":"paragraph"}],[{"inlineContent":[],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"switch"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"For switch components like "},{"code":"OUDSSwitch","type":"codeVoice"},{"type":"text","text":" and "},{"code":"OUDSSwitchItem","type":"codeVoice"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"tag"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"For tag components like "},{"code":"OUDSTag","type":"codeVoice"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"inputTag"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"For input tag components like "},{"code":"OUDSInputTag","type":"codeVoice"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"textArea"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"For text area components like "},{"code":"OUDSTextArea","type":"codeVoice"}],"type":"paragraph"}]],[[{"inline