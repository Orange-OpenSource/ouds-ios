{"variants":[{"paths":["\/documentation\/oudstokenscomponent"],"traits":[{"interfaceLanguage":"swift"}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"identifier":{"url":"doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent","interfaceLanguage":"swift"},"hierarchy":{"paths":[[]]},"metadata":{"role":"collection","title":"Tokens for components","symbolKind":"module","modules":[{"name":"Tokens for components"}],"externalID":"OUDSTokensComponent","images":[{"identifier":"ic_design_token_figma_component.svg","type":"icon"}],"roleHeading":"Library"},"kind":"symbol","topicSections":[{"anchor":"Group","title":"Group","identifiers":["doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/BadgeComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/BulletListComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/ButtonComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/CheckboxComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/ChipComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/ControlItemComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/DividerComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/LinkComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/PinCodeInputComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/QuantityInputComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/RadioButtonComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/SelectInputComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/SkeletonComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/SwitchComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/TagComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/TagInputComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/TextAreaComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/TextInputComponentTokens"]},{"generated":true,"anchor":"Protocols","title":"Protocols","identifiers":["doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/ButtonMonoComponentTokens","doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/LinkMonoComponentTokens"]}],"abstract":[{"text":"These ","type":"text"},{"inlineContent":[{"type":"text","text":"tokens"}],"type":"emphasis"},{"text":" are defined for components and refer to semantic tokens or raw tokens.","type":"text"},{"text":" ","type":"text"},{"text":"They can be seen as a kind of bridge between components and in the end primitive values of the design kit.","type":"text"}],"primaryContentSections":[{"kind":"content","content":[{"level":2,"text":"Overview","anchor":"Overview","type":"heading"},{"syntax":null,"code":["ðŸ§¬ Core version: 1.5.0"],"type":"codeListing"},{"inlineContent":[{"text":"Thus if a component need to change for example its ","type":"text"},{"inlineContent":[{"text":"background color","type":"text"}],"type":"emphasis"},{"text":", and if a ","type":"text"},{"inlineContent":[{"type":"text","text":"component token"}],"type":"emphasis"},{"text":" is used for it, then only the value of this ","type":"text"},{"inlineContent":[{"type":"text","text":"token"}],"type":"emphasis"},{"text":" should be changed without any modification on the ","type":"text"},{"inlineContent":[{"text":"component","type":"text"}],"type":"emphasis"},{"text":" definition.","type":"text"},{"text":" ","type":"text"},{"inlineContent":[{"type":"text","text":"Components"}],"type":"emphasis"},{"text":" use ","type":"text"},{"inlineContent":[{"text":"component tokens","type":"text"}],"type":"emphasis"},{"text":" exposed through the ","type":"text"},{"inlineContent":[{"type":"text","text":"theme"}],"type":"emphasis"},{"text":" thanks to token providers to get their style values.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Components tokens are defined and gathered inside "},{"inlineContent":[{"type":"text","text":"Swift protocol"}],"type":"emphasis"},{"type":"text","text":" so as to be exposed then through implementation of "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" thanks to dedicated tokens providers."},{"type":"text","text":" "},{"type":"text","text":"Because we choose to split responsabilities and objects into their own modules, we faced troubles to make possible for children themes or providers to override properties declared in "},{"inlineContent":[{"text":"protocols","type":"text"}],"type":"emphasis"},{"type":"text","text":" and defined in "},{"inlineContent":[{"type":"text","text":"extensions"}],"type":"emphasis"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"That is the reason why tokens are exposed as "},{"type":"codeVoice","code":"@objc open"},{"type":"text","text":" to be available and overridable anywhere."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Example with "},{"type":"reference","identifier":"doc:\/\/OUDSTokensComponent\/documentation\/OUDSTokensComponent\/ButtonComponentTokens","isActive":true},{"type":"text","text":":"}],"type":"paragraph"},{"syntax":"swift","code":["\/\/ Declare some component tokens for buttons with properties they must apply","public protocol ButtonComponentTokens {","    var buttonSizeMaxHeightIconOnly: SizeSemanticToken { get }","    var buttonBorderWidthDefault: BorderWidthSemanticToken { get }","    var buttonBorderRadius: BorderRadiusSemanticToken { get }","    var buttonColorBgDefaultPressedMono: MultipleColorSemanticTokens { get }","    var buttonSpacePaddingBlock: SpaceSemanticToken { get }","}","","\/\/ There is an existing provider for such tokens","open class OrangeThemeButtonComponentTokensProvider { }","","\/\/ Define the components tokens exposed through the theme thanks to provider","extension OrangeThemeButtonComponentTokensProvider: ButtonComponentTokens {","","    \/\/ Raw tokens can be used","","    @objc open var buttonSizeMaxHeightIconOnly: SizeSemanticToken { DimensionRawTokens.dimension600 }","    ","    \/\/ And also semantic tokens","","    @objc open var buttonBorderWidthDefault: BorderWidthSemanticToken { borders.borderWidthThicker }","    @objc open var buttonBorderRadius: BorderRadiusSemanticToken { borders.borderRadiusMedium }","    @objc open var buttonColorBgDefaultPressedMono: MultipleColorSemanticTokens { colors.colorRepositoryOpacityBlackHigher }","    @objc open var buttonSpacePaddingBlock: SpaceSemanticToken { spaces.spacePaddingInlineSpacious }","}","","\/\/ This provider is then exposed through OUDSTheme as an AllButtonComponentTokensProvider"],"type":"codeListing"},{"level":2,"text":"How to use component tokens","anchor":"How-to-use-component-tokens","type":"heading"},{"inlineContent":[{"type":"text","text":"In fact, the component tokens are declared and gathered in "},{"inlineContent":[{"type":"text","text":"Swift protocol"}],"type":"emphasis"},{"type":"text","text":" so as to force any theme to implement them, and also to allow any theme to expose such properties whathever the implementation of the theme is, across providers."},{"type":"text","text":" "},{"type":"text","text":"Because "},{"inlineContent":[{"text":"component tokens","type":"text"}],"type":"emphasis"},{"type":"text","text":" have for values "},{"inlineContent":[{"text":"raw tokens","type":"text"}],"type":"emphasis"},{"type":"text","text":" or "},{"inlineContent":[{"text":"semantic tokens","type":"text"}],"type":"emphasis"},{"type":"text","text":", and in the end for values primitive types, and all these tokens are declared with "},{"inlineContent":[{"type":"text","text":"type aliases"}],"type":"emphasis"},{"text":" refering all together, you can handle a ","type":"text"},{"inlineContent":[{"type":"text","text":"component token"}],"type":"emphasis"},{"text":" directly in your view because the final value will be used.","type":"text"},{"text":" ","type":"text"},{"text":"Thus, get the theme and call the needed property with some helpers.","type":"text"}],"type":"paragraph"},{"syntax":"swift","code":["struct SomeView: View {","","    @Environment(\\.theme) private var theme \/\/ Supposed you used in your root view the `OUDSThemeableView` to register the theme","    @Environment(\\.colorScheme) private var colorScheme","","    var body: some View {","        Button {","            \/\/ Some action","        } label: {","            Text(\"Some label\")","                .cornerRadius(theme.button.buttonBorderRadius)","        }","        .padding(.leading, theme.button.buttonSpacePaddingInlineChevronStart)","        .padding(.trailing, theme.button.buttonSpacePaddingInlineChevronEnd)","        .background(theme.button.buttonColorBgStrongEnabledMono)","        .overlay(","            RoundedRectangle(cornerRadius: theme.button.buttonBorderRadius)","                .stroke(Color.blue, lineWidth: theme.button.buttonBorderWidthDefault)","        )","    }","\/*","    - The theme provides border semantic tokens \"buttonBorderRadius\" and \"buttonBorderWidthDefault\"","    - The theme provides a color semantic token \"buttonColorBgStrongEnabledMono\"","    - The theme provides spaces semantic token \"buttonSpacePaddingInlineChevronStart\" and \"buttonSpacePaddingInlineChevronEnd\"","*\/","}","","\/\/ Do not forget in your app to use the `OUDSThemeableView` for your theme, e.g. `OrangeTheme`","@main","struct DesignToolbox: App {","","    var body: some Scene {","        WindowGroup {","            OUDSThemeableView(theme: OrangeTheme()) {","                \/\/ Your root view","                ...","            }","        }","    }","}"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"The components tokens are wrapped in tokens provider acessible through the themes:"}],"type":"paragraph"},{"rows":[[[{"type":"paragraph","inlineContent":[{"text":"Components tokens provider","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Target component"}]}]],[[{"type":"paragraph","inlineContent":[{"text":"badge","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"For badges like "},{"type":"codeVoice","code":"OUDSBadge"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"button"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"For buttons like "},{"type":"codeVoice","code":"OUDSButton"}]}]],[[{"type":"paragraph","inlineContent":[{"text":"bulletList","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"For list of elements with bullets"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"checkbox"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"For checkboxes like "},{"type":"codeVoice","code":"OUDSCheckbox"},{"type":"text","text":", "},{"type":"codeVoice","code":"OUDSCheckboxItem"},{"type":"text","text":" and their indeterminate variants"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"chip"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"For chips like "},{"type":"codeVoice","code":"OUDSSuggestionChip"},{"type":"text","text":" and "},{"type":"codeVoice","code":"OUDSFilterChip"}]}]],[[{"type":"paragraph","inlineContent":[{"text":"controlItem","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Shared and sued for switch, checkbox and radio items layouts"}]}]],[[{"type":"paragraph","inlineContent":[{"text":"divider","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"For dividers like "},{"type":"codeVoice","code":"OUDSHorizontalDivider"},{"type":"text","text":" and "},{"type":"codeVoice","code":"OUDSVerticalDivider