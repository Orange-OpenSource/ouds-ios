":"blur"}]},{"type":"text","text":", the "},{"type":"emphasis","inlineContent":[{"type":"text","text":"X offset"}]},{"type":"text","text":" and the "},{"type":"emphasis","inlineContent":[{"text":"Y offset","type":"text"}]},{"type":"text","text":", all of them being "},{"type":"reference","identifier":"doc:\/\/OUDSTokensRaw\/documentation\/OUDSTokensRaw\/ElevationRawToken","isActive":true},{"type":"text","text":" type."},{"type":"text","text":" "},{"type":"text","text":"Combinations of these three values can be defined inside "},{"type":"reference","identifier":"doc:\/\/OUDSTokensRaw\/documentation\/OUDSTokensRaw\/ElevationCompositeRawToken","isActive":true},{"type":"text","text":" i.e. "},{"type":"emphasis","inlineContent":[{"type":"text","text":"composite tokens"}]},{"type":"text","text":" defining a whil box shadow to use for elevation effect."}],"title":"ElevationRawTokens"},"doc://OUDSTokensRaw/documentation/OUDSTokensRaw/ElevationCompositeRawToken":{"navigatorTitle":[{"kind":"identifier","text":"ElevationCompositeRawToken"}],"title":"ElevationCompositeRawToken","role":"symbol","url":"\/documentation\/oudstokensraw\/elevationcompositerawtoken","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"ElevationCompositeRawToken"}],"type":"topic","identifier":"doc:\/\/OUDSTokensRaw\/documentation\/OUDSTokensRaw\/ElevationCompositeRawToken","kind":"symbol","abstract":[{"type":"text","text":"In the "},{"inlineContent":[{"type":"text","text":"Figma"}],"type":"emphasis"},{"type":"text","text":" global design system, composite tokens are defined for elevation effects."},{"type":"text","text":" "},{"type":"text","text":"Composite tokens are tokens defined in "},{"inlineContent":[{"type":"text","text":"Figma"}],"type":"emphasis"},{"type":"text","text":" by other tokens."},{"type":"text","text":" "},{"type":"text","text":"Here an "},{"inlineContent":[{"text":"elevation","type":"text"}],"type":"emphasis"},{"type":"text","text":" is caracterized by "},{"inlineContent":[{"type":"text","text":"X and Y offsets"}],"type":"emphasis"},{"type":"text","text":", a "},{"inlineContent":[{"text":"radius","type":"text"}],"type":"emphasis"},{"type":"text","text":" for the shadow and a "},{"inlineContent":[{"type":"text","text":"color"}],"type":"emphasis"},{"type":"text","text":" to apply on that shadow."},{"type":"text","text":" "},{"type":"text","text":"All these elements are "},{"inlineContent":[{"text":"raw tokens","type":"text"}],"type":"emphasis"},{"type":"text","text":", and together define a "},{"inlineContent":[{"text":"composite raw token","type":"text"}],"type":"emphasis"},{"type":"text","text":" for "},{"inlineContent":[{"type":"text","text":"elevation"}],"type":"emphasis"},{"type":"text","text":" effect."},{"type":"text","text":" "},{"type":"text","text":"It is defined as a "},{"code":"final class","type":"codeVoice"},{"type":"text","text":" and "},{"code":"NSObject","type":"codeVoice"},{"type":"text","text":" so as to be shared through "},{"code":"@objc","type":"codeVoice"},{"type":"text","text":" with extensions and protocols within modules."}]}}}