vigatorTitle":[{"text":"OUDSCheckbox","kind":"identifier"}],"externalID":"s:14OUDSComponents12OUDSCheckboxV"},"hierarchy":{"paths":[["doc:\/\/OUDSComponents\/documentation\/OUDSComponents"]]},"primaryContentSections":[{"declarations":[{"tokens":[{"text":"@","kind":"attribute"},{"text":"MainActor","kind":"attribute","preciseIdentifier":"s:ScM"},{"text":" ","kind":"text"},{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OUDSCheckbox","kind":"identifier"}],"platforms":["macOS"],"languages":["swift"]}],"kind":"declarations"},{"content":[{"text":"Indicator states","type":"heading","anchor":"Indicator-states","level":2},{"inlineContent":[{"text":"This checkbox indicator has two available states:","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"selected"}]},{"type":"text","text":": the checkbox is filled with a tick, the user has made the action to select the checkbox"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"inlineContent":[{"type":"text","text":"unselected"}],"type":"strong"},{"text":": the checkbox is empty, does not contain a tick, the user has made the action to unselect or did not select yet the checkbox","type":"text"}],"type":"paragraph"}]}]},{"inlineContent":[{"type":"text","text":"If you need to use a tri-state checkbox instead, refer to "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxIndeterminate"},{"type":"text","text":"."}],"type":"paragraph"},{"text":"Particular cases","type":"heading","anchor":"Particular-cases","level":2},{"inlineContent":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckbox"},{"type":"text","text":" can be related to an error situation, for example troubles for a formular."},{"type":"text","text":" "},{"type":"text","text":"A dedicated look-and-feel is implemented for that if the "},{"type":"codeVoice","code":"isError"},{"type":"text","text":" flag is risen."}],"type":"paragraph"},{"text":"Accessibility considerations","type":"heading","anchor":"Accessibility-considerations","level":2},{"inlineContent":[{"type":"text","text":"Note also the component must be instanciated with a string parameter used as accessibility label."},{"type":"text","text":" "},{"type":"text","text":"It is a good pratice (at least) to define a label for a component without text for accessibility reasons. This label will be vocalized by "},{"inlineContent":[{"type":"text","text":"Voice Over"}],"type":"emphasis"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"The vocalization tool will also use, after the label, a description of the component (if disabled, if error context), and a fake trait for checkbox."},{"type":"text","text":" "},{"type":"text","text":"No accessibility identifier is defined in OUDS side as this value remains in the users hands."}],"type":"paragraph"},{"text":"Cases forbidden by design","type":"heading","anchor":"Cases-forbidden-by-design","level":2},{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"The design system does not allow to have both an error situation and a disabled component."}]}],"type":"paragraph"},{"text":"Code samples","type":"heading","anchor":"Code-samples","level":2},{"syntax":"swift","code":["    \/\/ Supposing we have a selected checkbox","    @Published var isOn: Bool = true","","    \/\/ A simple checkbox, no error, not in read only mode","    OUDSCheckbox(isOn: $isOn, accessibilityLabel: \"The cake is a lie\")","","    \/\/ A simple checkbox, but is an error context","    OUDSCheckbox(isOn: $isOn, accessibilityLabel: \"The cake is a lie\"), isError: true)","","    \/\/ Never disable an error-related checkbox as it will crash","    \/\/ This is forbidden by design!","    OUDSCheckbox(isOn: $isOn, accessibilityLabel: \"The cake is a lie\"), isError: true).disabled(true) \/\/ fatal error"],"type":"codeListing"},{"text":"Suggestions","type":"heading","anchor":"Suggestions","level":2},{"inlineContent":[{"type":"text","text":"According to the "},{"type":"reference","isActive":true,"identifier":"https:\/\/unified-design-system.orange.com\/472794e18\/p\/09d860-checkbox\/t\/14bf4bd854"},{"type":"text","text":", the checkbox by default must be used in unselected state."}],"type":"paragraph"},{"text":"Design documentation","type":"heading","anchor":"Design-documentation","level":2},{"inlineContent":[{"type":"reference","identifier":"https:\/\/unified-design-system.orange.com\/472794e18\/p\/09d860-checkbox","isActive":true}],"type":"paragraph"},{"text":"Themes rendering","type":"heading","anchor":"Themes-rendering","level":2},{"text":"Orange","type":"heading","anchor":"Orange","level":3},{"inlineContent":[{"identifier":"component_checkbox_enabled_selected_Orange","type":"image"}],"type":"paragraph"},{"text":"Orange Business Tools","type":"heading","anchor":"Orange-Business-Tools","level":3},{"inlineContent":[{"identifier":"component_checkbox_enabled_selected_OrangeBusinessTools","type":"image"}],"type":"paragraph"},{"text":"Orange Inverse","type":"heading","anchor":"Orange-Inverse","level":3},{"inlineContent":[{"identifier":"component_checkbox_enabled_selected_OrangeInverse","type":"image"}],"type":"paragraph"},{"text":"Sosh","type":"heading","anchor":"Sosh","level":3},{"inlineContent":[{"identifier":"component_checkbox_enabled_selected_Sosh","type":"image"}],"type":"paragraph"},{"text":"Wireframe","type":"heading","anchor":"Wireframe","level":3},{"inlineContent":[{"type":"image","identifier":"component_checkbox_enabled_selected_Wireframe"}],"type":"paragraph"},{"content":[{"inlineContent":[{"type":"text","text":"2.0.0 (Figma component design version)"}],"type":"paragraph"}],"name":"Version","type":"aside","style":"note"},{"content":[{"type":"paragraph","inlineContent":[{"text":"0.12.0","type":"tex