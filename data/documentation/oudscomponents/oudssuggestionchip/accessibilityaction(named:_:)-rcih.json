xt"},{"text":"This version of the ","type":"text"},{"inlineContent":[{"type":"text","text":"suggestion chip"}],"type":"emphasis"},{"text":" uses the ","type":"text"},{"inlineContent":[{"text":"text only","type":"text"}],"type":"emphasis"},{"text":" layout which displays only text, offering a clean and minimalistic look.","type":"text"},{"text":" ","type":"text"},{"text":"Best suited for category-based filters that do not require additional visual elements.","type":"text"},{"text":" ","type":"text"},{"text":"Other layouts are available for this component like: ","type":"text"},{"inlineContent":[{"text":"text + icon","type":"text"}],"type":"emphasis"},{"text":" and ","type":"text"},{"inlineContent":[{"text":"icon only","type":"text"}],"type":"emphasis"},{"text":".","type":"text"}],"url":"\/documentation\/oudscomponents\/oudssuggestionchip","type":"topic","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OUDSSuggestionChip","kind":"identifier"}],"navigatorTitle":[{"text":"OUDSSuggestionChip","kind":"identifier"}],"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSSuggestionChip","title":"OUDSSuggestionChip","kind":"symbol"}}}