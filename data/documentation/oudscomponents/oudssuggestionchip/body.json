.","type":"text"},{"text":" ","type":"text"},{"text":"Other layouts are available for this component like: ","type":"text"},{"inlineContent":[{"type":"text","text":"text + icon"}],"type":"emphasis"},{"text":" and ","type":"text"},{"type":"emphasis","inlineContent":[{"text":"icon only","type":"text"}]},{"type":"text","text":"."}],"type":"topic","navigatorTitle":[{"text":"OUDSSuggestionChip","kind":"identifier"}],"title":"OUDSSuggestionChip","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OUDSSuggestionChip","kind":"identifier"}],"url":"\/documentation\/oudscomponents\/oudssuggestionchip","role":"symbol","identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSSuggestionChip","kind":"symbol"}}}