k? = .mineralWater","","    \/\/ Here the picker with title, selection and data","    OUDSChipPicker(title: Select a drink, selection: $selection, chips: someDataToPopulate)","","    \/\/ MARK: - Simple selection","","    \/\/ Prepare the selection, with a value previously selected","    \/\/ Use the data and the binding for the picker.","    @State var selection: Drink = .mineralWater","","    \/\/ Here the picker with title, selection and data","    OUDSChipPicker(title: Select a drink, selection: $selection, chips: someDataToPopulate)","","    \/\/ MARK: - Multiple selection","","    \/\/ Prepare the selection, with a value previously selected","    \/\/ Use the data and the binding for the picker.","    @State var selections: [Drink] = [.mineralWater]","","    \/\/ Here the picker with title, selection and data","    OUDSChipPicker(title: Select a drink, selections: $selections, chips: someDataToPopulate)"]}]}],"variants":[{"paths":["\/documentation\/oudscomponents\/controls"],"traits":[{"interfaceLanguage":"swift"}]}],"references":{"doc://OUDSComponents/documentation/OUDSComponents/OUDSSuggestionChip":{"role":"symbol","kind":"symbol","title":"OUDSSuggestionChip","type":"topic","url":"\/documentation\/oudscomponents\/oudssuggestionchip","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OUDSSuggestionChip","kind":"identifier"}],"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSSuggestionChip","abstract":[{"text":"Chips help people enter information, make selections, filter content, or trigger actions.","type":"text"},{"text":" ","type":"text"},{"text":"Chips can show multiple interactive elements together in the same area, such as a list of selectable movie times, or a serie of email contacts.","type":"text"},{"text":" ","type":"text"},{"text":"A ","type":"text"},{"type":"emphasis","inlineContent":[{"type":"text","text":"suggestion chip"}]},{"text":" is a compact UI element used to present recommended or pr