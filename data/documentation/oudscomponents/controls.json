cted","    \/\/ Use the data and the binding for the picker.","    @State var selection: Drink? = .mineralWater","","    \/\/ Here the picker with title, selection and data","    OUDSChipPicker(title: Select a drink, selection: $selection, chips: someDataToPopulate)","","    \/\/ MARK: - Simple selection","","    \/\/ Prepare the selection, with a value previously selected","    \/\/ Use the data and the binding for the picker.","    @State var selection: Drink = .mineralWater","","    \/\/ Here the picker with title, selection and data","    OUDSChipPicker(title: Select a drink, selection: $selection, chips: someDataToPopulate)","","    \/\/ MARK: - Multiple selection","","    \/\/ Prepare the selection, with a value previously selected","    \/\/ Use the data and the binding for the picker.","    @State var selections: [Drink] = [.mineralWater]","","    \/\/ Here the picker with title, selection and data","    OUDSChipPicker(title: Select a drink, selections: $selections, chips: someDataToPopulate)"]}],"kind":"content"}],"references":{"doc://OUDSComponents/documentation/OUDSComponents/OUDSRadioItem":{"url":"\/documentation\/oudscomponents\/oudsradioitem","kind":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OUDSRadioItem","kind":"identifier"}],"navigatorTitle":[{"text":"OUDSRadioItem","kind":"identifier"}],"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSRadioItem","type":"topic","role":"symbol","abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSRadioItem"},{"text":" proposes layouts to add in your views some radios components.","type":"text"},{"text":" ","type":"text"},{"text":"If you want to use a radio with only an indicator, prefer instead ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSRadio"},{"text":".","type":"te