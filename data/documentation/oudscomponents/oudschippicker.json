{"hierarchy":{"paths":[["doc:\/\/OUDSComponents\/documentation\/OUDSComponents"]]},"variants":[{"paths":["\/documentation\/oudscomponents\/oudschippicker"],"traits":[{"interfaceLanguage":"swift"}]}],"topicSections":[{"anchor":"Initializers","title":"Initializers","generated":true,"identifiers":["doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSChipPicker\/init(title:selection:chips:)-83b2u","doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSChipPicker\/init(title:selection:chips:)-8zbwb","doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSChipPicker\/init(title:selections:chips:)"]},{"anchor":"Instance-Properties","title":"Instance Properties","generated":true,"identifiers":["doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSChipPicker\/body"]},{"anchor":"Default-Implementations","title":"Default Implementations","generated":true,"identifiers":["doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSChipPicker\/View-Implementations","doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSChipPicker\/View-Implementations"]}],"abstract":[{"type":"text","text":"A picker allowing to expose several "},{"type":"emphasis","inlineContent":[{"type":"text","text":"filter chips"}]},{"type":"text","text":" and proposing two kinds of selection:"}],"sections":[],"kind":"symbol","schemaVersion":{"patch":0,"major":0,"minor":3},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSChipPicker"},"metadata":{"title":"OUDSChipPicker","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OUDSChipPicker"}],"navigatorTitle":[{"kind":"identifier","text":"OUDSChipPicker"}],"roleHeading":"Structure","symbolKind":"struct","role":"symbol","modules":[{"name":"OUDSComponents"}],"externalID":"s:14OUDSComponents14OUDSChipPickerV"},"relationshipsSections":[{"title":"Conforms To","kind":"relationships","identifiers":["doc:\/\/OUDSComponents\/s8SendableP",