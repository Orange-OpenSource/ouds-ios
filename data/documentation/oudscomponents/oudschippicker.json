n case","","    \/\/ Prepare the selection, with a value previously selected","    \/\/ Use the data and the binding for the picker.","    @State var selections: [Drink] = [.mineralWater]","","    \/\/ Here the picker with title, selection and data","    OUDSChipPicker(title: \"Select a drink\", selections: $selections, chips: someDataToPopulate)"]},{"type":"heading","level":2,"anchor":"Design-documentation","text":"Design documentation"},{"type":"paragraph","inlineContent":[{"text":"There is no online specification as this component is not an official OUDS one.","type":"text"}]},{"type":"heading","level":2,"anchor":"Theme-rendering","text":"Theme rendering"},{"type":"heading","level":3,"anchor":"Orange","text":"Orange"},{"type":"paragraph","inlineContent":[{"type":"image","identifier":"component_chipPicker_Orange"}]},{"type":"heading","level":3,"anchor":"Orange-Compact","text":"Orange Compact"},{"type":"paragraph","inlineContent":[{"identifier":"component_chipPicker_OrangeCompact","type":"image"}]},{"type":"heading","level":3,"anchor":"Sosh","text":"Sosh"},{"type":"paragraph","inlineContent":[{"type":"image","identifier":"component_chipPicker_Sosh"}]},{"type":"heading","level":3,"anchor":"Wireframe","text":"Wireframe"},{"type":"paragraph","inlineContent":[{"identifier":"component_chipPicker_Wireframe","type":"image"}]},{"style":"note","name":"Since","content":[{"inlineContent":[{"text":"0.17.0","type":"text"}],"type":"paragraph"}],"type":"aside"}]}],"abstract":[{"text":"A picker allowing to expose several ","type":"text"},{"inlineContent":[{"type":"text","text":"filter chips"}],"type":"emphasis"},{"text":" and proposing two kinds of selection:","type":"text"}],"hierarchy":{"paths":[["doc:\/\/OUDSComponents\/documentation\/OUDSComponents"]]},"metadata":{"title":"OUDSChipPicker","symbolKind":"struct","modules":[{"name":"Components"}],"role":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OUDSChipPicker"