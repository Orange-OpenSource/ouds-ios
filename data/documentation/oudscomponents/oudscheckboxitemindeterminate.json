{"identifier":{"url":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItemIndeterminate","interfaceLanguage":"swift"},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/oudscomponents\/oudscheckboxitemindeterminate"]}],"metadata":{"symbolKind":"struct","externalID":"s:14OUDSComponents29OUDSCheckboxItemIndeterminateV","role":"symbol","modules":[{"name":"OUDSComponents"}],"roleHeading":"Structure","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OUDSCheckboxItemIndeterminate"}],"navigatorTitle":[{"text":"OUDSCheckboxItemIndeterminate","kind":"identifier"}],"title":"OUDSCheckboxItemIndeterminate"},"schemaVersion":{"minor":3,"major":0,"patch":0},"hierarchy":{"paths":[["doc:\/\/OUDSComponents\/documentation\/OUDSComponents"]]},"topicSections":[{"identifiers":["doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItemIndeterminate\/init(selection:label:helper:icon:flipIcon:isReversed:isError:isReadOnly:hasDivider:action:)"],"title":"Initializers","generated":true,"anchor":"Initializers"},{"identifiers":["doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItemIndeterminate\/body"],"title":"Instance Properties","generated":true,"anchor":"Instance-Properties"},{"identifiers":["doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItemIndeterminate\/View-Implementations","doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItemIndeterminate\/View-Implementations"],"title":"Default Implementations","generated":true,"anchor":"Default-Implementations"}],"kind":"symbol","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItemIndeterminate","isActive":true,"type":"reference"},{"text":" proposes layouts to add in your views some checkboxes components.","type":"text"},{"text":" ","type":"text"},{"text":"If you want to use a checkbox with only an indicator prefer instead ","type":"text"},{"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxIndeterminate","isActive":true,"type":"reference"},{"text":".","type":"text"}],"sections":[],"primaryContentSections":[{"declarations":[{"languages":["swift"],"tokens":[{"kind":"attribute","text":"@"},{"kind":"attribute","preciseIdentifier":"s:ScM","text":"MainActor"},{"kind":"text","text":" "},{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OUDSCheckboxItemIndeterminate"}],"platforms":["macOS"]}],"kind":"declarations"},{"content":[{"type":"heading","text":"Layouts","level":2,"anchor":"Layouts"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The component can be rendered as two different layouts:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"default"}],"type":"strong"},{"text":": the component has a leading indicator, a label and optional helper texts, and an optional trailing decorative icon","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"inverse","type":"text"}],"type":"strong"},{"text":": like the ","type":"text"},{"inlineContent":[{"type":"text","text":"default"}],"type":"emphasis"},{"text":" layout but with a trailing checkbox indicator and a leading optional image","type":"text"}]}]}]},{"type":"heading","text":"Indicator states","level":2,"anchor":"Indicator-states"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The checkbox indicator has three available states:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"selected"}]},{"type":"text","text":": the checkbox is filled with a tick, the user has made the action to select the checkbox"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"unselected"}],"type":"strong"},{"type":"text","text":": the checkbox is empty, does not contain a tick, the user has made the action to unselect or did not select yet the checkbox"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"indeterminate","type":"text"}]},{"text":": mike a prefilled or preticked checkbox, the user did not do anything on it yet","type":"text"}]}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If you need a checkbox with only two states to manage use instead "},{"type":"reference","identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItem","isActive":true},{"type":"text","text":"."}]},{"level":2,"anchor":"Particular-cases","text":"Particular cases","type":"heading"},{"inlineContent":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItemIndeterminate"},{"type":"text","text":" can be related to an error situation, for example troubles for a formular."},{"type":"text","text":" "},{"type":"text","text":"A dedicated look and feel is implemented for that if the "},{"type":"codeVoice","code":"isError"},{"type":"text","text":" flag is risen."},{"type":"text","text":" "},{"type":"text","text":"In addition, the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItemIndeterminate"},{"type":"text","text":" can be in read only mode, i.e. the user cannot interact with the component yet"},{"type":"text","text":" "},{"type":"text","text":"but this component must not be considered as disabled."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The component does not follow the right-to-left (RTL) \/ left-to-right (LTR) mode returned by the system as it could have some meaning"},{"type":"text","text":" "},{"type":"text","text":"to have for example the indicator in trailing position for LTR mode and vice versa."},{"type":"text","text":" "},{"type":"text","text":"However, if the component has an icon in leading position (RTL mode) or in trailing position (LTR), the content of the icon is never changed."},{"type":"text","text":" "},{"type":"text","text":"It could lead to a loss of meaning or semantics in the icon. Thus a specific flag can be used to flip the icon content whatever the layout direction is."},{"type":"text","text":" "},{"type":"text","text":"It prevents the user do implement its own rules to flip or not image."}],"type":"paragraph"},{"level":2,"anchor":"Accessibility-considerations","text":"Accessibility considerations","type":"heading"},{"inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"Voice Over"}]},{"type":"text","text":" will use several elements to describe the component: if component disabled \/ read only, if error context, the label and helper texts and a custom checkbox trait."}],"type":"paragraph"},{"level":2,"anchor":"Forbidden-by-design","text":"Forbidden by design","type":"heading"},{"inlineContent":[{"inlineContent":[{"text":"The design system does not allow to have both an error situation and a read only component.","type":"text"}],"type":"strong"},{"text":" ","type":"text"},{"inlineContent":[{"type":"text","text":"The design system does not allow to have both an error situation and a disabled component."}],"type":"strong"},{"text":" ","type":"text"},{"inlineContent":[{"text":"The design system does not allow to have both a read only and a disabled component.","type":"text"}],"type":"strong"}],"type":"paragraph"},{"level":2,"anchor":"Code-samples","text":"Code samples","type":"heading"},{"code":["    \/\/ Supposing we have an indeterminate state checkbox","    @Published var selection: OUDSCheckboxIndicatorState = .indeterminate","","    \/\/ A leading checkbox with a label.","    \/\/ The default layout will be used here.","    OUDSCheckboxItemIndeterminate(selection: $selection, label: \"Hello world\")","","    \/\/ A leading checkbox with a label, but in read only mode (user cannot interact yet, but not disabled).","    \/\/ The default layout will be used here.","    OUDSCheckboxItemIndeterminate(selection: $selection, label: \"Hello world\", isReadOnly: true)","","    \/\/ A leading checkbox with a label, and an helper text.","    \/\/ The default layout will be used here.","    OUDSCheckboxItemIndeterminate(selection: $selection, label: \"Bazinga!\", helper: \"Doll-Dagga Buzz-Buzz Ziggety-Zag\")","","    \/\/ A trailing checkbox with a label, an helper text and an icon.","    \/\/ The reversed layout will be used here.","    OUDSCheckboxItemIndeterminate(selection: $selection,","                                  label: \"We live in a fabled world\",","                                  helper: \"Of dreaming boys and wide-eyed girls\",","                                  isReversed: true,","                                  icon: Image(decorative: \"ic_heart\"))","","    \/\/ A leading checkbox with a label, but disabled.","    \/\/ The default layout will be used here.","    OUDSCheckboxItemIndeterminate(selection: $selection, label: \"Hello world\")","        .disabled(true)","","    \/\/ A leading checkbox with a label and and icon but with the icon flipped vertically","    OUDSCheckboxItemIndeterminate(isOn: $selection,","                                  labelText: \"Cocorico !\",","                                  icon: Image(systemName: \"figure.handball\"),","                                  flipIcon: true)","","    \/\/ Never disable a read only or an error-related checkbox as it will crash","    \/\/ This is forbidden by design!","    OUDSCheckboxItemIndeterminate(selection: $selection, label: \"Hello world\", isError: true).disabled(true) \/\/ fatal error","    OUDSCheckboxItemIndeterminate(selection: $selection, label: \"Hello world\", isReadyOnly: true).disabled(true) \/\/ fatal error"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"If you want to manage the RTL mode quite easily and switch your layouts (flip image, indicator in RTL leading i.e. in the right):"}],"type":"paragraph"},{"code":["    @Environment(\\.layoutDirection) var layoutDirection","","    OUDSCheckboxItemIndeterminate(isOn: $selection,","                                  labelText: \"Cocorico !\",","                                  icon: Image(systemName: \"figure.handball\"),","                                  flipIcon: layoutDirection == .rightToLeft,","                                  isInversed: layoutDirection == .rightToLeft)"],"syntax":"swift","type":"codeListing"},{"level":2,"anchor":"Suggestions","text":"Suggestions","type":"heading"},{"inlineContent":[{"type":"text","text":"According to the "},{"isActive":true,"type":"reference","identifier":"https:\/\/unified-design-system.orange.com\/472794e18\/p\/09d860-checkbox\/t\/14bf4bd854"},{"type":"text","text":", the checkbox by default must be used in unselected state."}],"type":"paragraph"},{"level":2,"anchor":"Design-documentation","text":"Design documentation","type":"heading"},{"inlineContent":[{"isActive":true,"identifier":"https:\/\/unified-design-system.orange.com\/472794e18\/p\/09d860-checkbox","type":"reference"}],"type":"paragraph"},{"level":2,"anchor":"Themes-rendering","text":"Themes rendering","type":"heading"},{"level":3,"anchor":"Orange","text":"Orange","type":"heading"},{"inlineContent":[{"type":"image","identifier":"component_checkbox_item_Orange"}],"type":"paragraph"},{"level":3,"anchor":"Orange-Business-Tools","text":"Orange Business Tools","type":"heading"},{"inlineContent":[{"type":"image","identifier":"component_checkbox_item_OrangeBusinessTools"}],"type":"paragraph"},{"level":3,"anchor":"Orange-Inverse","text":"Orange Inverse","type":"heading"},{"inlineContent":[{"type":"image","identifier":"component_checkbox_item_OrangeInverse"}],"type":"paragraph"},{"level":3,"anchor":"Sosh","text":"Sosh","type":"heading"},{"inlineContent":[{"type":"image","identifier":"component_checkbox_item_Sosh"}],"type":"paragraph"},{"level":3,"anchor":"Wireframe","text":"Wireframe","type":"heading"},{"inlineContent":[{"identifier":"component_checkbox_item_Wireframe","type":"image"}],"type":"paragraph"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"2.0.0 (Figma component design version)"}]}],"style":"note","name":"Version","type":"aside"},{"content":[{"inlineContent":[{"type":"text","text":"0.12.0"}],"type":"paragraph"}],"style":"note","name":"Since","type":"aside"}],"kind":"content"}],"relationshipsSections":[{"identifiers":["doc:\/\/OUDSComponents\/s8SendableP","doc:\/\/OUDSComponents\/7SwiftUI4ViewP"],"title":"Conforms To","type":"conformsTo","kind":"relationships"}],"references":{"doc://OUDSComponents/documentation/OUDSComponents/OUDSCheckbox":{"type":"topic","title":"OUDSCheckbox","url":"\/documentation\/oudscomponents\/oudscheckbox","role":"symbol","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckbox"},{"type":"text","text":" proposes layout to add in your views a lonely checkbox, without labels, texts nor icons components."},{"type":"text","text":" "},{"type":"text","text":"If you want to use a checkbox with additional texts and icon, prefer instead "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckboxItem"},{"type":"text","text":"."}],"kind":"symbol","navigatorTitle":[{"text":"OUDSCheckbox","kind":"identifier"}],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OUDSCheckbox","kind":"identifier"}],"identifier":"doc:\/\/OUDSComponents\/documentation\/OUDSComponents\/OUDSCheckbox"},"doc://OUDSComponents/7SwiftUI4ViewP":{"type":"unresolvable","identifier":"doc:\/\/OUDSComponents\/7SwiftUI4ViewP","title":"SwiftUICore.View"},"doc://OUDSComponents/documentation/OUDSComponents/OUDSCheckboxItemIndeterminate/init(selec