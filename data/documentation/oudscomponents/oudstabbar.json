ss_Wireframe_dark":{"alt":"A  tab bar component in dark mode with Liquid Glass effect and Wireframe theme","type":"image","identifier":"component_tabBar_LiquidGlass_Wireframe_dark","variants":[{"traits":["1x","light"],"url":"\/images\/OUDSComponents\/component_tabBar_LiquidGlass_Wireframe_dark.png"}]},"component_tabBar_LiquidGlass_Orange_light":{"alt":"A tab bar component in light mode with Liquid Glass effect and Orange theme","type":"image","identifier":"component_tabBar_LiquidGlass_Orange_light","variants":[{"url":"\/images\/OUDSComponents\/component_tabBar_LiquidGlass_Orange_light.png","traits":["1x","light"]}]}}}