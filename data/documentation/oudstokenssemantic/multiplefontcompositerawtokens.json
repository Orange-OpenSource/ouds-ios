{"schemaVersion":{"major":0,"patch":0,"minor":3},"topicSections":[{"generated":true,"title":"Initializers","anchor":"Initializers","identifiers":["doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/MultipleFontCompositeRawTokens\/init(_:)","doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/MultipleFontCompositeRawTokens\/init(compact:regular:)"]},{"generated":true,"title":"Instance Properties","anchor":"Instance-Properties","identifiers":["doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/MultipleFontCompositeRawTokens\/compact","doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/MultipleFontCompositeRawTokens\/regular"]},{"generated":true,"title":"Instance Methods","anchor":"Instance-Methods","identifiers":["doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/MultipleFontCompositeRawTokens\/fontToken(for:)","doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/MultipleFontCompositeRawTokens\/isEqual(_:)"]}],"metadata":{"externalID":"c:@M@OUDSTokensSemantic@objc(cs)MultipleFontCompositeRawTokens","role":"symbol","modules":[{"name":"OUDSTokensSemantic"}],"navigatorTitle":[{"kind":"identifier","text":"MultipleFontCompositeRawTokens"}],"title":"MultipleFontCompositeRawTokens","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"MultipleFontCompositeRawTokens"}],"roleHeading":"Class","symbolKind":"class"},"abstract":[{"type":"text","text":"Kind of semantic tokens which will wrap a combination of "},{"type":"codeVoice","code":"FontCompositeRawToken"},{"type":"text","text":" depending to size classes."},{"type":"text","text":" "},{"type":"text","text":"Kind of composite token with multiple values, but not named “composite” because this word is already used in the design system."},{"type":"text","text":" "},{"type":"text","text":"Allows to gather the multiple-value tokens from "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Figma"}]},{"type