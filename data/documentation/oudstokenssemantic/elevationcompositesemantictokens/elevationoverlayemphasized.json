sSemantic\/documentation\/OUDSTokensSemantic\/ElevationColorSemanticToken"},{"type":"text","text":")."},{"type":"text","text":" "},{"type":"text","text":"However the composite tokens (here for box shadows) are defined in "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/ElevationCompositeSemanticTokens"},{"type":"text","text":" because the "},{"type":"emphasis","inlineContent":[{"text":"tokenator","type":"text"}]},{"type":"text","text":" "},{"type":"text","text":"is not able to generate them yet, and they must be defined elsewhere to not be deleted."},{"type":"text","text":" "},{"type":"text","text":"Any elevation semantic token must be declared there (except "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/ElevationCompositeSemanticTokens"},{"type":"text","text":")"}],"type":"topic","role":"symbol","identifier":"doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/ElevationSemanticTokens","url":"\/documentation\/oudstokenssemantic\/elevationsemantictokens","title":"ElevationSemanticTokens","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"ElevationSemanticTokens","kind":"identifier"}],"kind":"symbol"},"ic_design_token_figma_semantic.png":{"identifier":"ic_design_token_figma_semantic.png","type":"image","alt":null,"variants":[{"url":"\/images\/OUDSTokensSemantic\/ic_design_token_figma_semantic.png","traits":["1x","light"]}]},"doc://OUDSTokensSemantic/documentation/OUDSTokensSemantic/MultipleElevationCompositeRawTokens":{"url":"\/documentation\/oudstokenssemantic\/multipleelevationcompositerawtokens","type":"topic","navigatorTitle":[{"text":"MultipleElevationCompositeRawTokens","kind":"identifier"}],"role":"symbol","kind":"symbol","identifier":"doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/MultipleElevationCompositeRawTokens","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"MultipleElevationCompositeRawTokens","kind":"identifier"}],"title":"MultipleElevationCompositeRawTokens","abstract":[{"type":"text","text":"Semantic tokens which will wrap a combination of "},{"type":"codeVoice","code":"ElevationCompositeRawToken"},{"type":"text","text":" depending to color scheme (i.e. light mode or dark mode)"},{"type":"text","text":" "},{"type":"text","text":"Kind of composite token with multiple values, but not named “composite” because this word is already used in the design system."},{"type":"text","text":" "},{"type":"text","text":"Allows to gather the multiple-value tokens from "},{"inlineContent":[{"type":"text","text":"Figma"}],"type":"emphasis"},{"type":"text","text":" inside one object."},{"type":"text","text":" "},{"type":"text","text":"If an elevation token exists with its value depending to the color scheme, it must be packed in such ``MultipleElevationCompositeRawTokens`"}]},"doc://OUDSTokensSemantic/documentation/OUDSTokensSemantic/ElevationCompositeSemanticTokens":{"identifier":"doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/ElevationCompositeSemanticTokens","type":"topic","fragments":[{"text":"protocol","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"ElevationCompositeSemanticTokens"}],"navigatorTitle":[{"kind":"identifier","text":"ElevationCompositeSemanticTokens"}],"url":"\/documentation\/oudstokenssemantic\/elevationcompositesemantictokens","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"This is a group of semantic tokens for "},{"type":"strong","inlineContent":[{"text":"elevations","type":"text"}]},{"type":"text","text":", but only "},{"type":"strong","inlineContent":[{"text":"composite tokens","type":"text"}]},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"There are splitted and not declared in "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/ElevationSemanticTokens"},{"type":"text","text":" as the "},{"type":"emphasis","inlineContent":[{"type":"text","text":"tokenator"}]},{"type":"text","text":" tool parsing "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Figma"}]},{"type":"text","text":" JSON to Swift code is not able to manage them."},{"type":"text","text":" "},{"type":"text","text":"Thus we need to declare them in another file to prevent them to be erased."},{"type":"text","text":" "},{"type":"text","text":"It declares in fact box shadows effects."}],"title":"ElevationCompositeSemanticTokens"}}}