ifier":"doc:\/\/OUDSTokensSemantic\/documentation\/OUDSTokensSemantic\/MultipleElevationCompositeRawTokens","abstract":[{"type":"text","text":"Semantic tokens which will wrap a combination of "},{"type":"codeVoice","code":"ElevationCompositeRawToken"},{"type":"text","text":" depending to color scheme (i.e. light mode or dark mode)"},{"type":"text","text":" "},{"type":"text","text":"Kind of composite token with multiple values, but not named “composite” because this word is already used in the design system."},{"type":"text","text":" "},{"type":"text","text":"Allows to gather the multiple-value tokens from "},{"type":"emphasis","inlineContent":[{"text":"Figma","type":"text"}]},{"type":"text","text":" inside one object."},{"type":"text","text":" "},{"type":"text","text":"If an elevation token exists with its value depending to the color scheme, it must be packed in such ``MultipleElevationCompositeRawTokens`"}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"MultipleElevationCompositeRawTokens"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"MultipleElevationCompositeRawTokens"}]},"doc://OUDSTokensRaw/documentation/OUDSTokensRaw/ElevationCompositeRawToken":{"abstract":[{"type":"text","text":"In the "},{"type":"emphasis","inlineContent":[{"text":"Figma","type":"text"}]},{"type":"text","text":" global design system, composite tokens are defined for elevation effects."},{"type":"text","text":" "},{"type":"text","text":"Composite tokens are tokens defined in "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Figma"}]},{"type":"text","text":" by other tokens."},{"type":"text","text":" "},{"type":"text","text":"Here an "},{"type":"emphasis","inlineContent":[{"text":"elevation","type":"text"}]},{"type":"text","text":" is caracterized by "},{"type":"emphasis","inlineContent":[{"type":"text","text":"X and Y offsets"}]},{"type":"text","text":", a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"radius"}]},{"type":"text","text":" for the shadow and a "},{"type":"emphasis","inlineContent":[{"text":"color","type":"text"}]},{"type":"text","text":" to apply on that shadow."},{"type":"text","text":" "},{"type":"text","text":"All these elements are "},{"type":"emphasis","inlineContent":[{"type":"text","text":"raw tokens"}]},{"type":"text","text":", and together define a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"composite raw token"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"elevation","type":"text"}]},{"type":"text","text":" effect."},{"type":"text","text":" "},{"type":"text","text":"It is defined as a "},{"type":"codeVoice","code":"final class"},{"type":"text","text":" and "},{"type":"codeVoice","code":"NSObject"},{"type":"text","text":" so as to be shared through "},{"type":"codeVoice","code":"@objc"},{"type":"text","text":" with extensions and protocols within modules."}],"type":"topic","role":"symbol","title":"ElevationCompositeRawToken","kind":"symbol","identifier":"doc:\/\/OUDSTokensRaw\/documentation\/OUDSTokensRaw\/ElevationCompositeRawToken","url":"\/documentation\/oudstokensraw\/elevationcompositerawtoken","fragments":[{"text":"final","kind":"keyword"},{"text":" ","kind":"text"},{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"ElevationCompositeRawToken","kind":"identifier"}]},"doc://OUDSTokensSemantic/documentation/OUDSTokensSemantic/MultipleElevationCompositeRawTokens/light":{"title":"light","abstract":[{"text":"For ","type":"text"},{"inlineContent":[{"type":"text","text":"light"}],"type":"strong"},{"text":" mode scheme","type":"text"}],"fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"light","kind":"identifier"},{"text":": ","kind":"text"},{"text":"ElevationCompositeRawToken","kind":"typeIdentifier","preciseIdentifier":"c:@M@OUDSTokensRaw@objc(cs)ElevationCompositeRawToken"}],"type":"topic","url":"\/documentation\/oudstokenssemantic