":"emphasis","inlineContent":[{"type":"text","text":"raw tokens"}]},{"type":"text","text":", and together define a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"composite raw token"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"elevation","type":"text"}]},{"type":"text","text":" effect."},{"type":"text","text":" "},{"type":"text","text":"It is defined as a "},{"type":"codeVoice","code":"final class"},{"type":"text","text":" and "},{"type":"codeVoice","code":"NSObject"},{"type":"text","text":" so as to be shared through "},{"type":"codeVoice","code":"@objc"},{"type":"text","text":" with extensions and protocols within modules."}],"type":"topic","role":"symbol","title":"ElevationCompositeRawToken","kind":"symbol","identifier":"doc:\/\/OUDSTokensRaw\/documentation\/OUDSTokensRaw\/ElevationCompositeRawToken","url":"\/documentation\/oudstokensraw\/elevationcompositerawtoken","fragments":[{"text":"final","kind":"keyword"},{"text":" ","kind":"text"},{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"ElevationCompositeRawToken","kind":"identifier"}]}}}