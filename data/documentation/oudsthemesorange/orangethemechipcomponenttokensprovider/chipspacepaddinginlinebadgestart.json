ovider","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeChipComponentTokensProvider"}],"type":"topic","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens of chip","type":"text"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"chip"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSFilterChip"},{"type":"text","text":" and "},{"type":"codeVoice","code":"OUDSSuggestionChip"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllChipComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeChipComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"ChipComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"text":"chip","type":"text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Chip","type":"text"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") ,"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":"), dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and border  (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"role":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeChipComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemechipcomponenttokensprovider","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeChipComponentTokensProvider"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeChipComponentTokensProvider/chipSpacePaddingInlineBadgeStart":{"title":"chipSpacePaddingInlineBadgeStart","role":"symbol","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeChipComponentTokensProvider\/chipSpacePaddingInlineBadgeStart","url":"\/documentation\/oudsthemesorange\/orangethemechipcomponenttokensprovider\/chipspacepaddinginlinebadgestart","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"chipSpacePaddingInlineBadgeStart"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"SpaceSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena"}],"abstract":[{"text":"Has for value token spaces.spacePaddingInline4xs","type":"text"}],"type":"topic"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","url":"\/documentation\/oudsthemesorange","kind":"symbol","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","type":"topic","title":"OUDSThemesOrange"}}}