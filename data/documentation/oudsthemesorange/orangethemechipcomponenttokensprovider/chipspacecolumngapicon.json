type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":"), dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and border  (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"title":"OrangeThemeChipComponentTokensProvider","kind":"symbol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeChipComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeChipComponentTokensProvider","role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemechipcomponenttokensprovider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeChipComponentTokensProvider"}],"type":"topic"},"ic_theme_orange.png":{"variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png","traits":["1x","light"]}],"type":"image","identifier":"ic_theme_orange.png","alt":null},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeChipComponentTokensProvider/chipSpaceColumnGapIcon":{"abstract":[{"text":"Has for value token spaces.spaceColumnGapXs","type":"text"}],"url":"\/documentation\/oudsthemesorange\/orangethemechipcomponenttokensprovider\/chipspacecolumngapicon","title":"chipSpaceColumnGapIcon","role":"symbol","kind":"symbol","type":"topic","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"chipSpaceColumnGapIcon","kind":"identifier"},{"text":": ","kind":"text"},{"text":"SpaceSemanticToken","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeChipComponentTokensProvider\/chipSpaceColumnGapIcon"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","abstract":[{"type":"text","te