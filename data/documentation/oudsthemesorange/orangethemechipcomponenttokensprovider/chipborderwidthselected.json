ype":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeChipComponentTokensProvider","type":"reference","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"ChipComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"chip"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Chip"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") ,"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":"), dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and border  (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"title":"OrangeThemeChipComponentTokensProvider","kind":"symbol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeChipComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeChipComponentTokensProvider","role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemechipcomponenttokensprovider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeChipComponentTokensProvider"}],"type":"topic"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrang