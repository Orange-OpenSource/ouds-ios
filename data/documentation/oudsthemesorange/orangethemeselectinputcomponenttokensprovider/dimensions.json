images":[{"identifier":"ic_theme_orange.png","type":"icon"}],"type":"topic","abstract":[{"text":"The Orange theme overrides some tokens from the basic ","type":"text"},{"type":"codeVoice","code":"OUDSTheme"},{"text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example.","type":"text"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeSelectInputComponentTokensProvider":{"fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeSelectInputComponentTokensProvider","kind":"identifier"}],"title":"OrangeThemeSelectInputComponentTokensProvider","role":"symbol","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens of select input","type":"text"}]},{"type":"text","text":" for objects like "},{"type":"codeVoice","code":"OUDSSelectInput"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllSelectInputComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSelectInputComponentTokensProvider","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"SelectInputComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"select input"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Select input"}]},{"type":"text","text":" components tokens are defined with semantic tokens of dimensions (from "},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSelectInputComponentTokensProvider","type":"topic","navigatorTitle":[{"text":"OrangeThemeSelectInputComponentTokensProvider","kind":"identifier"}],"url":"\/documentation\/oudsthemesorange\/orangethemeselectinputcomponenttokensprovider","kind":"symbol"},"doc://OUDS/documentation/OUDS/AllDimensionSemanticTokensProvider":{"abstract":[{"type":"text","text":"Something which provides all semantic tokens of dimension"}],"identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllDimensionSemanticTokensProvider","type":"topic","title":"AllDimensionSemanticTokensProvider","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"AllDimensionSemanticTokensProvider"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","preciseIdentifier":"s:4OUDS14TokensProviderP","text":"TokensProvider"},{"kind":"text","text":", "},{"kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic09DimensionB6TokensP","text":"DimensionSemanticTokens"}],"role":"symbol","url":"\/documentation\/ouds\/alldimensionsemantictokensprovider","kind":"symbol"},"ic_theme_orange.png":{"type":"image","identifier":"ic_theme_orange.png","alt":null,"variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png","traits":["1x","light"]}]},"doc://OUDSThemesOrange/docu