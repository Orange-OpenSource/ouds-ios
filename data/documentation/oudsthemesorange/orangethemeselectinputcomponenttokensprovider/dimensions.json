ocol "},{"code":"SelectInputComponentTokens","type":"codeVoice"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"text":"select input","type":"text"}]},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Select input","type":"text"}]},{"type":"text","text":" components tokens are defined with semantic tokens of dimensions (from "},{"code":"AllDimensionSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")"}],"title":"OrangeThemeSelectInputComponentTokensProvider","type":"topic","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSelectInputComponentTokensProvider","role":"symbol"}}}