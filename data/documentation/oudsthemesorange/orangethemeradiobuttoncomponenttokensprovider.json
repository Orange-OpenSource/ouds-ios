e"},{"type":"text","text":" to your own theme implementation:"}],"type":"paragraph"},{"syntax":"swift","code":["class LocalTheme: OrangeTheme {","","    override init() {","        super.init(radioButton: CustomRadioButtonComponentTokensProvider())","    }","}"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"or to an already existing theme for example:"}],"type":"paragraph"},{"syntax":"swift","code":["    OrangeTheme(radioButton: CustomRadioButtonComponentTokensProvider())"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"It is also possible to use your own semantic tokens providers for this component tokens providers:"}],"type":"paragraph"},{"syntax":"swift","code":["    \/\/ Uses by default here:","    \/\/ - OrangeThemeSizeSemanticTokensProvider for sizes","    \/\/ - OrangeThemeBorderSemanticTokensProvider for borders","    let radioButtonComponentTokensProvider = OrangeThemeRadioButtonComponentTokensProvider()","","    \/\/ Or use your own size, border, color and opacity semantic tokens providers (or only some)","    let radioButtonComponentTokensProvider = OrangeThemeRadioButtonComponentTokensProvider(","                                                sizes: CustomSizeSemanticTokensProvider(),","                                                borders: CustomBorderSemanticTokensProvider()))"],"type":"codeListing"},{"name":"Since","content":[{"type":"paragraph","inlineContent":[{"text":"0.12.0","type":"text"}]}],"type":"aside","style":"note"}]}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeRadioButtonComponentTokensProvider"},"relationshipsSections":[{"kind":"relationships","type":"conformsTo","title":"Conforms To","identifiers":["doc:\/\/OUDS\/documentation\/OUDS\/AllRadioButtonComponentTokensProvider","doc:\/\/OUDSThemesOrange\/19OUDSTokensComponent011RadioButtonB6TokensP","doc:\/\/OUDSThemesOrange\/s8CopyableP","doc:\/\/OUDS\/documentation\/OUDS\/TokensPro