{"schemaVersion":{"minor":3,"major":0,"patch":0},"identifier":{"url":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of tag"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"tag","type":"text"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSTag"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllTagComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"TagComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"tags"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Tags"}]},{"type":"text","text":" components tokens are defined with semantic tokens of dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":") ,"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":") , sizes (from "},{"type":"codeVoice","code":"AllSizesSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"]]},"relationshipsSections":[{"identifiers":["doc:\/\/OUDS\/documentation\/OUDS\/AllTagComponentTokensProvider","doc:\/\/OUDSThemesOrange\/19OUDSTokensComponent03TagB6TokensP","doc:\/\/OUDSThemesOrange\/s8CopyableP","doc:\/\/OUDS\/documentation\/OUDS\/TokensProvider"],"kind":"relationships","title":"Conforms To","type":"conformsTo"}],"primaryContentSections":[{"declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTagComponentTokensProvider"}]}],"kind":"declarations"},{"kind":"content","content":[{"type":"heading","level":2,"text":"Overview","anchor":"overview"},{"type":"codeListing","code":["    \/\/ Define your own provider for tag component tokens","    \/\/ by inheriting from existing provider","    class CustomTagComponentTokensProvider: OrangeThemeTagComponentTokensProvider {","","        \/\/ Then override the tag component tokens you want.","","        override var tagBorderRadius: BorderRadiusSemanticToken  { borders.borderRadiusNone }","","        override var tagSizeMinWidthMedium: SizeSemanticToken { DimensionRawTokens.dimension500 }","","        override var tagSpacePaddingInlineIconStartSmall: SpaceSemanticToken { spaces.spacePaddingInlineMd }","","        \/\/ ...","    }","","    \/\/ Or define your own provider from scratch","    class CustomTagComponentTokensProvider: TagComponentTokens {","","        \/\/ And implement maybe hundreds of tokens.","        \/\/ You are allowed to use semantic tokens providers if you want to define values.","    }"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Then, you can give this "},{"code":"CustomTagComponentTokensProvider","type":"codeVoice"},{"text":" to your own theme implementation:","type":"text"}]},{"type":"codeListing","code":["class LocalTheme: OrangeTheme {","","    override init() {","        super.init(tag: CustomTagComponentTokensProvider())","    }","}"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"or to an already existing theme for example:"}]},{"type":"codeListing","code":["    OrangeTheme(tag: CustomTagComponentTokensProvider())"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"text":"It is also possible to use your own semantic tokens providers for this component tokens providers:","type":"text"}]},{"type":"codeListing","code":["    \/\/ Uses by default here:","    \/\/ - OrangeThemeSizeSemanticTokensProvider for sizes","    \/\/ - OrangeThemeBorderSemanticTokensProvider for borders","    \/\/ - OrangeThemeDimensionSemanticTokensProvider for dimensions","    \/\/ - OrangeThemeSpaceSemanticTokensProvider for spaces","    let tagComponentTokensProvider = OrangeThemeTagComponentTokensProvider()","","    \/\/ Or use your own size, space, dimension and border semantic tokens providers (or only some)","    let tagComponentTokensProvider = OrangeThemeTagComponentTokensProvider(","                                            sizes: CustomSizeSemanticTokensProvider(),","                                            borders: CustomBorderSemanticTokensProvider(),","                                            spaces: CustomSpaceSemanticTokensProvider,","                                    