heme: OrangeTheme {","","    override init() {","        super.init(button: CustomButtonComponentTokensProvider())","    }","}"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"or to an already existing theme for example:"}],"type":"paragraph"},{"syntax":"swift","code":["    OrangeTheme(button: CustomButtonComponentTokensProvider())"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"It is also possible to use your own semantic tokens providers for this component tokens providers:"}],"type":"paragraph"},{"syntax":"swift","code":["    \/\/ Uses by default here:","    \/\/ - OrangeThemeSizeSemanticTokensProvider for sizes","    \/\/ - OrangeThemeBorderSemanticTokensProvider for borders","    \/\/ - OrangeThemeColorSemanticTokensProvider for colors","    \/\/ - OrangeThemeSpaceSemanticTokensProvider for spaces","    let buttonComponentTokensProvider = OrangeThemeButtonComponentTokensProvider()","","    \/\/ Or use your own size, borders, colors and space semantic tokens providers (or only some)","    let buttonComponentTokensProvider = OrangeThemeButtonComponentTokensProvider(","                                                sizes: CustomSizeSemanticTokensProvider(),","                                                borders: CustomBorderSemanticTokensProvider","                                                colors: CustomColorSemanticTokensProvider","                                                spaces: CustomSpaceSemanticTokensProvider())"],"type":"codeListing"},{"name":"Since","content":[{"inlineContent":[{"type":"text","text":"0.9.0"}],"type":"paragraph"}],"style":"note","type":"aside"}],"kind":"content"}],"hierarchy":{"paths":[["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"]]},"variants":[{"paths":["\/documentation\/oudsthemesorange\/orangethemebuttoncomponenttokensprovider"],"traits":[{"interfaceLanguage":"swift"}]}],"metadata":{"title":"OrangeThemeButtonComponentTokensProvider","roleHeading":"Class","modules":[{"name":