lements also the protocol "},{"type":"codeVoice","code":"PinceCodeInputComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"text":"pin code input","type":"text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Pin code input"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of dimensions (from "},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemepincodeinputcomponenttokensprovider","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemePinCodeInputComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemePinCodeInputComponentTokensProvider/spaces":{"role":"symbol","title":"spaces","type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemePinCodeInputComponentTokensProvider\/spaces","url":"\/documentation\/oudsthemesorange\/orangethemepincodeinputcomponenttokensprovider\/spaces","abstract":[{"text":"Provider of spaces semantic tokens to use for pin code input spaces","type":"text"}],"fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"spaces","kind":"identifier"},{"text":": any ","kind":"text"},{"text":"AllSpaceSemanticTokensProvider","preciseIdentifier":"s:4OUDS30AllSpaceSemanticTokensProviderP","kind":"typeIdentifier"}],"kind":"symbol"},"doc://OUDS/documentation/OUDS/AllSpaceSemanticTokensProvider":{"type":"topic","kind":"symbol","role":"symbol","title":"AllSpaceSemanticTokensProvider","url":"\/documentation\/ouds\/alls