{"sections":[],"schemaVersion":{"major":0,"minor":3,"patch":0},"identifier":{"url":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemePinCodeInputComponentTokensProvider\/spaces","interfaceLanguage":"swift"},"metadata":{"roleHeading":"Instance Property","fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"spaces","kind":"identifier"},{"text":": any ","kind":"text"},{"preciseIdentifier":"s:4OUDS30AllSpaceSemanticTokensProviderP","text":"AllSpaceSemanticTokensProvider","kind":"typeIdentifier"}],"symbolKind":"property","modules":[{"name":"Theme Orange"}],"externalID":"s:16OUDSThemesOrange0B40ThemePinCodeInputComponentTokensProviderC6spaces4OUDS016AllSpaceSemantichI0_pvp","role":"symbol","title":"spaces"},"kind":"symbol","variants":[{"paths":["\/documentation\/oudsthemesorange\/orangethemepincodeinputcomponenttokensprovider\/spaces"],"traits":[{"interfaceLanguage":"swift"}]}],"abstract":[{"type":"text","text":"Provider of spaces semantic tokens to use for pin code input spaces"}],"primaryContentSections":[{"declarations":[{"platforms":["macOS"],"tokens":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"spaces"},{"kind":"text","text":": any "},{"kind":"typeIdentifier","text":"AllSpaceSemanticTokensProvider","identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllSpaceSemanticTokensProvider","preciseIdentifier":"s:4OUDS30AllSpaceSemanticTokensProviderP"}],"languages":["swift"]}],"kind":"declarations"}],"hierarchy":{"paths":[["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemePinCodeInputComponentTokensProvider"]]},"references":{"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemePinCodeInputComponentTokensProvider":{"abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of pin code input"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSPinCodeInput"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllPinCodeInputComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemePinCodeInputComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"PinceCodeInputComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"pin code input"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Pin code input"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of dimensions (from "},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"kind":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemePinCodeInputComponentTokensProvider","kind":"identifier"}],"url":"\/documentation\/oudsthemesorange\/orangethemepincodeinputcomponenttokensprovider","title":"OrangeThemePinCodeInputComponentTokensProvider","role":"symbol","type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemePinCodeInputComponentTokensProvider","navigatorTitle":[{"text":"OrangeThemePinCodeInputComponentTokensProvider","kind":"identifier"}]},"doc://OUDS/documentation/OUDS/AllSpaceSemanticTokensProvider":{"url":"\/documentation\/ouds\/allspacesemantictokensprovider","kind":"symbol","abstract":[{"type":"text","text":"Something which provides all semantic tokens of space"}],"role":"symbol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"AllSpaceSemanticTokensProvider"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","preciseIdentifier":"s:4OUDS14TokensProviderP","text":"TokensProvider"},{"kind":"text","text":", "},{"kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic013SpaceMultipleB6TokensP","text":"SpaceMultipleSemanticTokens"},{"kind":"text","text":", "},{"kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB6TokensP","text":"SpaceSemanticTokens"}],"type":"topic","title":"AllSpaceSemanticTokensProvider","identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllSpaceSemanticTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","images":[{"identifier":"ic_theme_orange.png","type":"icon"}],"kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","url":"\/documentati