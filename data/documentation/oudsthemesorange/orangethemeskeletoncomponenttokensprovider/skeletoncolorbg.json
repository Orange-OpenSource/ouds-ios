n"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Skeleton"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")."}],"navigatorTitle":[{"kind":"identifier","text":"OrangeThemeSkeletonComponentTokensProvider"}],"title":"OrangeThemeSkeletonComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeSkeletonComponentTokensProvider/skeletonColorBg":{"title":"skeletonColorBg","url":"\/documentation\/oudsthemesorange\/orangethemeskeletoncomponenttokensprovider\/skeletoncolorbg","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSkeletonComponentTokensProvider\/skeletonColorBg","kind":"symbol","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"skeletonColorBg"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens","text":"MultipleColorSemanticTokens"}],"abstract":[{"type":"text","text":"Has for value token colors.colorOpacityLowest"}],"type":"topic","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"kind":"symbol","role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example.","type":"text"}],"type":"topic","title":"OUDSThemesOrange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","url":"\/documentation\/oudsthemesorange"}}}