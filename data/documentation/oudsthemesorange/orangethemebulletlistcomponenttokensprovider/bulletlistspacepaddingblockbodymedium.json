ants":[{"traits":["1x","light"],"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.svg"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"images":[{"type":"icon","identifier":"ic_theme_orange.svg"}],"type":"topic","role":"collection","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","title":"Theme Orange","abstract":[{"text":"The Orange theme overrides some tokens from the basic ","type":"text"},{"type":"codeVoice","code":"OUDSTheme"},{"text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example.","type":"text"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeBulletListComponentTokensProvider/bulletListSpacePaddingBlockBodyMedium":{"abstract":[{"text":"Has for value token spaces.spacePaddingBlock2xs","type":"text"}],"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemebulletlistcomponenttokensprovider\/bulletlistspacepaddingblockbodymedium","title":"bulletListSpacePaddingBlockBodyMedium","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"bulletListSpacePaddingBlockBodyMedium"},{"kind":"text","text":": "},{"preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","kind":"typeIdentifier","text":"SpaceSemanticToken"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBulletListComponentTokensProvider\/bulletListSpacePaddingBlockBodyMedium","type":"topic","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeBulletListComponentTokensProvider":{"kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBulletListComponentTokensProvider","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of bullet list"}]},{"type":"text","text":" for "},{"type":"emphasis","inl