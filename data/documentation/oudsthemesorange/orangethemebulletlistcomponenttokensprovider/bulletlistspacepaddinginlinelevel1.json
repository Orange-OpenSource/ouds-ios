rough any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Bullet list"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"url":"\/documentation\/oudsthemesorange\/orangethemebulletlistcomponenttokensprovider","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeBulletListComponentTokensProvider"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"type":"topic","role":"collection","title":"OUDSThemesOrange","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeBulletListComponentTokensProvider/bulletListSpacePaddingInlineLevel1":{"fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"bulletListSpacePaddingInlineLevel1"},{"kind":"text","text":": "},{"preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","kind":"typeIdentifier","text":"SpaceSemanticToken"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBulletListComponentTokensProvider\/bulletListSpacePaddingInlineLevel1","url":"\/documentation\/oudsthemesorange\/orangethemebulletlistcomponenttokensprovider\/bulletlistspacepaddinginlinelevel1","role":"symbol","title":"bulletListSpacePaddingInlineLevel1","abstract":[{"text":"Has for value token DimensionRawTokens.dimension450","type":"text"}],"kind":"symbol","type":"t