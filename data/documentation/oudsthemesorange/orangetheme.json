rity reasons.","   let yourOwnOrangeTheme = OrangeTheme(colors: colors, borders: borders, sizes: sizes, spaces: spaces, button: button, badge: badge)",""],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Then get it:"}]},{"code":["    import OUDS","","    @Environment(\\.theme) var theme"],"type":"codeListing","syntax":"swift"},{"content":[{"inlineContent":[{"type":"text","text":"0.8.0"}],"type":"paragraph"}],"name":"Since","type":"aside","style":"note"}],"kind":"content"}],"variants":[{"paths":["\/documentation\/oudsthemesorange\/orangetheme"],"traits":[{"interfaceLanguage":"swift"}]}],"sections":[],"seeAlsoSections":[{"identifiers":["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeBrandColorRawTokens","doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeBrandFontRawTokens"],"generated":true,"anchor":"Group","title":"Group"}],"kind":"symbol","relationshipsSections":[{"identifiers":["doc:\/\/OUDS\/documentation\/OUDS\/OUDSTheme"],"kind":"relationships","type":"inheritsFrom","title":"Inherits From"},{"identifiers":["doc:\/\/OUDSThemesOrange\/s8SendableP"],"kind":"relationships","type":"conformsTo","title":"Conforms To"}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeTheme"},"schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"]]},"topicSections":[{"identifiers":["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeTheme\/init(borders:colors:colorModes:colorCharts:elevations:fonts:grids:opacities:dimensions:sizes:spaces:badge:bulletList:button:checkbox:chip:controlItem:divider:link:pinCodeInput:quantityInput:radioButton:selectInput:skeleton:switch:tag:tagInput:textArea:textInput:resourcesBundle:fontFamily:)"],"generated":true,"anchor":"Initializers","title":"Initializers"}],"references":{"doc://OUDSThemesOrange/documentation/OUDSTheme