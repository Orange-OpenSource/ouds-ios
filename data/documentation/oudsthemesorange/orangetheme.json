rimaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeTheme","kind":"identifier"}],"platforms":["macOS"],"languages":["swift"]}]},{"kind":"content","content":[{"text":"Usages","anchor":"Usages","type":"heading","level":1},{"inlineContent":[{"type":"text","text":"Any Orange products must use this theme. It provides core and Orange colors and any elements for sizings, spacings and dimensions."},{"type":"text","text":" "},{"type":"text","text":"The system font families are used; no "},{"type":"emphasis","inlineContent":[{"text":"Helevetica","type":"text"}]},{"type":"text","text":" font fmaily is embeded nor provider by OUDS."},{"type":"text","text":" "},{"type":"text","text":"This theme also provides colros charts charts."}],"type":"paragraph"},{"text":"Integration","anchor":"Integration","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"To use this theme, inject it to your view using "},{"type":"codeVoice","code":"OUDSThemeableView"},{"type":"text","text":" and get it through environment variable."}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["    import OUDS                 \/\/ To get OUDSThemeableView","    import OUDSThemesOrange    \/\/ To get OrangeTheme","    import SwiftUI","","    @main","    struct YourApp: App {","        var body: some Scene {","            WindowGroup {","                OUDSThemeableView(theme: OrangeTheme()) {","                    \/\/ Your root view","                }","            }","        }","    }"]},{"inlineContent":[{"text":"You can also use some tokens providers defined in your side, but they must match the same type as the one used on the themes (see ","type":"text"},{"type":"codeVoice","code":"OUDSTheme"},{"text":").","type":"text"},{"text":" ","type":"text"},{"text":"For example:","type":"text"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["","   \/\/ Use some already defined providers","   let borders = OrangeThemeBorderSemanticTokensProvider()","   let sizes = OrangeThemeSizeSemanticTokensProvider()","   let spaces = OrangeThemeSpaceSemanticTokensProvider()","","   \/\/ For example, use your own provider of color semantic tokens","   \/\/ It must inherit from OrangeThemeBorderSemanticTokensProvider (which is instance of AllColorSemanticTokensProvider) or directly implement AllColorSemanticTokensProvider","   let colors = YourOwnColorSemanticTokensProvider()","","   \/\/ Some providers need other providers to use their tokens","   let button = OrangeThemeButtonComponentTokensProvider(sizes: sizes, borders: borders, colors: colors, spaces: spaces)","","   \/\/ But you can also use the providers as they are without assigning semantic tokens providers","   \/\/ and just use the default ones","   let badge = OrangeThemeBadgeComponentTokensProvider()","","   \/\/ Define in the end your theme by overriding some providers.","   \/\/ You can override all providers, or only some, or just keep the theme as is.","   \/\/ However you should uses thhese providers in a dedicated Orange theme subclass for clarity reasons.","   let yourOwnOrangeTheme = OrangeTheme(colors: colors, borders: borders, sizes: sizes, spaces: spaces, button: button, badge: badge)",""]},{"inlineContent":[{"type":"text","text":"Then get it:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["    import OUDS","","    @Environment(\\.theme) var theme"]},{"content":[{"inlineContent":[{"text":"0.8.0","type":"text"}],"type":"paragraph"}],"name":"Since","type":"aside","style":"note"}]}],"metadata":{"role":"symbol","symbolKind":"class","roleHeading":"Class","navigatorTitle":[{"kind":"identifier","text":"OrangeTheme"}],"externalID":"s:16OUDSThemesOrange0B5ThemeC","modules":[{"name":"Theme Orange"}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeTheme"}],"title":"OrangeTheme"},"references":{"i