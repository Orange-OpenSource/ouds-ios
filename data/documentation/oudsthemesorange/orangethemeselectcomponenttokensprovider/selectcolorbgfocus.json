asis","inlineContent":[{"type":"text","text":"select"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Select"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":") and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeSelectComponentTokensProvider","kind":"identifier"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeSelectComponentTokensProvider/selectColorBgFocus":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSelectComponentTokensProvider\/selectColorBgFocus","title":"selectColorBgFocus","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"selectColorBgFocus","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens","text":"MultipleColorSemanticTokens","kind":"typeIdentifier"}],"abstract":[],"type":"topic","kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemeselectcomponenttokensprovider\/selectcolorbgfocus","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"url":"\/documentation\/oudsthemesorange","type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","abstract":[{"text":"The Orange theme overrides some tokens from the basic ","type":"text"},{"code":"OUDSTheme","type":"codeVoice"},{"text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example.","type":"text"}],"title":"OUDSThemesOrange","kind":"symbol","role":"collection"}}}