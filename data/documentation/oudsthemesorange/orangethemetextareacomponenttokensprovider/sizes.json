nt":[{"text":"text area","type":"text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Text area","type":"text"}]},{"type":"text","text":" components tokens are defined with semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":")."}],"title":"OrangeThemeTextAreaComponentTokensProvider","kind":"symbol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTextAreaComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextAreaComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTextAreaComponentTokensProvider/sizes":{"fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"sizes","kind":"identifier"},{"text":": any ","kind":"text"},{"preciseIdentifier":"s:4OUDS29AllSizeSemanticTokensProviderP","text":"AllSizeSemanticTokensProvider","kind":"typeIdentifier"}],"role":"symbol","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextAreaComponentTokensProvider\/sizes","title":"sizes","abstract":[{"type":"text","text":"Provider of sizez semantic tokens to use for text area borders"}],"url":"\/documentation\/oudsthemesorange\/orangethemetextareacomponenttokensprovider\/sizes","type":"topic"},"doc://OUDS/documentation/OUDS/AllSizeSemanticTokensProvider":{"type":"topic","role":"symbol","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AllSizeSemanticTokensProvider","kind":"identifier"},{"text":" : ","kind":"text"},{"text":"TokensProvider","preciseIdentifier":"s:4OUDS14TokensProviderP","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"SizeMultipleSemanticTokens","preciseIdentifier":"s:18OUDSTokensSemantic012SizeMult