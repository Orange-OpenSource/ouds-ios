om "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":")."}],"kind":"symbol","type":"topic","title":"OrangeThemeTextAreaComponentTokensProvider","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTextAreaComponentTokensProvider"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTextAreaComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextAreaComponentTokensProvider"}}}