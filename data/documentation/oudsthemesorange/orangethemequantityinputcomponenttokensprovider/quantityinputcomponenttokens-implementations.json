ent":[{"type":"text","text":"tag inputs"}]},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Quantity inputs"}]},{"type":"text","text":" components tokens are defined with semantic tokens of sizes (from "},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"title":"OrangeThemeQuantityInputComponentTokensProvider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeQuantityInputComponentTokensProvider"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeQuantityInputComponentTokensProvider/quantityInputSpaceColumnGapQuantitySelector":{"abstract":[{"type":"text","text":"Has for value token DimensionRawTokens.dimensionOutOfSystem25"}],"fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"quantityInputSpaceColumnGapQuantitySelector"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"SpaceSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeQuantityInputComponentTokensProvider\/quantityInputSpaceColumnGapQuantitySelector","url":"\/documentation\/oudsthemesorange\/orangethemequantityinputcomponenttokensprovider\/quantityinputspacecolumngapquantityselector","kind":"symbol","role":"symbol","title":"quantityInputSpaceColumnGapQuantitySelector","type":"topic"}}}