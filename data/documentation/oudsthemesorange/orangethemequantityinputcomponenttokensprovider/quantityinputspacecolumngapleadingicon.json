putComponentTokensProvider":{"navigatorTitle":[{"text":"OrangeThemeQuantityInputComponentTokensProvider","kind":"identifier"}],"kind":"symbol","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of quantity input"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSQuantityInput"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllQuantityInputComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeQuantityInputComponentTokensProvider","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"QuantityInputComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"tag inputs"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Quantity inputs"}]},{"type":"text","text":" components tokens are defined with semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"role":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeQuantityInputComponentTokensProvider","type":"topic","title":"OrangeThemeQuantityInputComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemequantityinputcomponenttokensprovider","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeQuantityInputComponentTokensProvider","kind":"identifier"}]},"ic_theme_orange.png":{"variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png","traits":["1x","light"]}],"type":"image","identifier":"ic_theme_orange.png","alt":null},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeQuantityInputComponentTokensProvider/quantityInputSpaceColumnGapLeadingIcon":{"abstract":[{"text":"Has for value token spaces.spaceColumnGapMd","type":"text"}],"url":"\/documentation\/oudsthemesorange\/orangethemequantityinputcomponenttokensprovider\/quantityinputspacecolumngapleadingicon","role":"symbol","kind":"symbol","type":"topic","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"quantityInputSpaceColumnGapLeadingIcon"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"SpaceSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena"}],"title":"quantityInputSpaceColumnGapLeadingIcon","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeQuantityInputComponentTokensProvider\/quantityInputSpaceColumnGapLeadingIcon"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"images":[{"type":"icon","identifier":"ic_theme_orange.png"}],"kind":"symbol","type":"topic","title":"Theme Orange","url":"\/documentation\/oudsthemesorange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"}}}