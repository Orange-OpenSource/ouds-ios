inlineContent":[{"text":"switch \/ toggle","type":"text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Switch"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpacesSemanticTokensProvider"},{"type":"text","text":") ,  dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"sizes ("},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders ("},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":") and opacities ("},{"type":"codeVoice","code":"AllOpacitiySemanticTokensProvider"},{"type":"text","text":")."}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeSwitchComponentTokensProvider/switchSizeMaxHeight":{"kind":"symbol","title":"switchSizeMaxHeight","type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/switchSizeMaxHeight","url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider\/switchsizemaxheight","role":"symbol","abstract":[{"text":"Has for value token dimensions.dimensionXs","type":"text"}],"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"switchSizeMaxHeight","kind":"identifier"},{"text":": ","kind":"text"},{"text":"SizeSemanticToken","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"url":"\/documentation\/oudsthemesorange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","title":"OUDSThemesOrange"}}}