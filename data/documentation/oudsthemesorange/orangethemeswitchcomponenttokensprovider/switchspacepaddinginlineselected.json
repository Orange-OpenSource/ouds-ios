sions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"sizes ("},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders ("},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":") and opacities ("},{"type":"codeVoice","code":"AllOpacitiySemanticTokensProvider"},{"type":"text","text":")."}],"type":"topic","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeSwitchComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider"}}}