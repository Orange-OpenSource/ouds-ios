e":"codeVoice","code":"AllSpacesSemanticTokensProvider"},{"type":"text","text":") , sizes ("},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders ("},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":") and opacities ("},{"type":"codeVoice","code":"AllOpacitiySemanticTokensProvider"},{"type":"text","text":")."}],"url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider","role":"symbol","navigatorTitle":[{"text":"OrangeThemeSwitchComponentTokensProvider","kind":"identifier"}]}}}