sionSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"sizes ("},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders ("},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":") and opacities ("},{"type":"codeVoice","code":"AllOpacitiySemanticTokensProvider"},{"type":"text","text":")."}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeSwitchComponentTokensProvider/sizes":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/sizes","kind":"symbol","type":"topic","role":"symbol","fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"sizes","kind":"identifier"},{"text":": any ","kind":"text"},{"text":"AllSizeSemanticTokensProvider","preciseIdentifier":"s:4OUDS29AllSizeSemanticTokensProviderP","kind":"typeIdentifier"}],"abstract":[{"type":"text","text":"Provider of size semantic tokens to use for switch sizes"}],"url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider\/sizes","title":"sizes"}}}