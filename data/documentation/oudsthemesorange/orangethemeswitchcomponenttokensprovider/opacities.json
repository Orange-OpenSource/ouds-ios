emesOrange":{"abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"role":"collection","images":[{"type":"icon","identifier":"ic_theme_orange.png"}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","title":"Theme Orange","kind":"symbol"},"doc://OUDSThemesContract/documentation/OUDSThemesContract/AllOpacitySemanticTokensProvider":{"url":"\/documentation\/oudsthemescontract\/allopacitysemantictokensprovider","abstract":[{"type":"text","text":"Something which provides all semantic tokens of opacity"}],"kind":"symbol","title":"AllOpacitySemanticTokensProvider","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"AllOpacitySemanticTokensProvider"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:18OUDSThemesContract14TokensProviderP","kind":"typeIdentifier","text":"TokensProvider"},{"kind":"text","text":", "},{"preciseIdentifier":"s:18OUDSTokensSemantic07OpacityB6TokensP","kind":"typeIdentifier","text":"OpacitySemanticTokens"}],"identifier":"doc:\/\/OUDSThemesContract\/documentation\/OUDSThemesContract\/AllOpacitySemanticTokensProvider","role":"symbol","type":"topic"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeSwitchComponentTokensProvider":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeSwitchComponentTokensProvider"}],"title":"OrangeThemeSwitchComponentTokensProvider","role":"symbol","type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider","abstract":[{"type":"text","text":"A class which wraps all "},{"inlineContent":[{"type":"text","text":"component  tokens of switch"}],"type":"strong"},{"type":"text","text":" for "},{"inlineContent":[{"text":"switch \/ toggle","type":"text"}],"type":"emphasis"},{"type":"text","text":" objects like "},{"code":"OUDSSwitch","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"code":"AllSwitchComponentTokensProvider","type":"codeVoice"},{"type":"text","text":" implementation inside "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"code":"SwitchComponentTokens","type":"codeVoice"},{"type":"text","text":" so as to expose the component tokens for "},{"inlineContent":[{"type":"text","text":"switch \/ toggle"}],"type":"emphasis"},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Switch"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"code":"AllSpacesSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":") ,  dimensions ("},{"code":"AllDimensionSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"sizes ("},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"), borders ("},{"code":"AllBorderSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":") and opacities ("},{"code":"AllOpacitiySemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeSwitchComponentTokensProvider"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeSwitchComponentTokensProvider/opacities":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/opacities","fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"opacities","kind":"identifier"},{"text":": any ","kind":"text"},{"text":"AllOpacitySemanticTokensProvider","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSThemesContract32AllOpacitySemanticTokensProviderP"}],"title":"opacities","role":"symbol","type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider\/opacities","abstract":[{"text":"Provider of opacities semantic tokens to use for switch spaces","type":"text"}],"kind":"symbol"}}}