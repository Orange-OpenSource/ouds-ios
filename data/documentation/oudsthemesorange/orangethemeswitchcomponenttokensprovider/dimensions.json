rotocol "},{"code":"SwitchComponentTokens","type":"codeVoice"},{"type":"text","text":" so as to expose the component tokens for "},{"inlineContent":[{"type":"text","text":"switch \/ toggle"}],"type":"emphasis"},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"inlineContent":[{"text":"Switch","type":"text"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"code":"AllSpacesSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":") ,  dimensions ("},{"code":"AllDimensionSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"sizes ("},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"), borders ("},{"code":"AllBorderSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":") and opacities ("},{"code":"AllOpacitiySemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider","title":"OrangeThemeSwitchComponentTokensProvider","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider","role":"symbol"}}}