ocumentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider\/borderradiustrack","title":"borderRadiusTrack","kind":"symbol","fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"borderRadiusTrack"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","text":"BorderRadiusSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic012BorderRadiusB5Tokena"}],"type":"topic","role":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/borderRadiusTrack"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","images":[{"type":"icon","identifier":"ic_theme_orange.png"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","type":"topic","kind":"symbol","abstract":[{"text":"The Orange theme overrides some tokens from the basic ","type":"text"},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"title":"Theme Orange","url":"\/documentation\/oudsthemesorange"},"ic_theme_orange.png":{"type":"image","alt":null,"identifier":"ic_theme_orange.png","variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png","traits":["1x","light"]}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeSwitchComponentTokensProvider":{"role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeSwitchComponentTokensProvider","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider","title":"OrangeThemeSwitchComponentTokensProvider","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens of switch","type":"text"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"switch \/ toggle","type":"text"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSSwitch"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllSwitchComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider","type":"reference","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"SwitchComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"text":"switch \/ toggle","type":"text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Switch","type":"text"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpacesSemanticTokensProvider"},{"type":"text","text":") ,  dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"sizes ("},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders ("},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":") and opacities ("},{"type":"codeVoice","code":"AllOpacitiySemanticTokensProvider"},{"type":"text","text":")."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider","type":"topic","navigatorTitle":[{"text":"OrangeThemeSwitchComponentTokensProvider","kind":"identifier"}]}}}