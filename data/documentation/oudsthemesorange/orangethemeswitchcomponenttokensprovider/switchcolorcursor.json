/documentation/OUDSThemesOrange/OrangeThemeSwitchComponentTokensProvider/switchColorCursor":{"type":"topic","title":"switchColorCursor","url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider\/switchcolorcursor","abstract":[{"type":"text","text":"Has for value token MultipleColorSemanticTokens(light: colors.colorRepositoryNeutralMutedWhite, dark: colors.colorRepositoryNeutralEmphasizedHighest)"}],"kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/switchColorCursor","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"switchColorCursor"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"MultipleColorSemanticTokens","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens"}],"role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeSwitchComponentTokensProvider":{"url":"\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider","role":"symbol","title":"OrangeThemeSwitchComponentTokensProvider","navigatorTitle":[{"text":"OrangeThemeSwitchComponentTokensProvider","kind":"identifier"}],"type":"topic","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeSwitchComponentTokensProvider","kind":"identifier"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider","kind":"symbol","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of switch"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"switch \/ toggle"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSSwitch"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllSwitchComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"SwitchComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"text":"switch \/ toggle","type":"text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Switch"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpacesSemanticTokensProvider"},{"type":"text","text":") ,  dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"sizes ("},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders ("},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":") and opacities ("},{"type":"codeVoice","code":"AllOpacitiySemanticTokensProvider"},{"type":"text","text":")."}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"url":"\/documentation\/oudsthemesorange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","title":"OUDSThemesOrange"}}}