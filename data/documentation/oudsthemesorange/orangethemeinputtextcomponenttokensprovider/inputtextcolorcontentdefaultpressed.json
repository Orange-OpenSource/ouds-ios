th raw and semantic tokens of sizes (from "},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"), colors ("},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemeinputtextcomponenttokensprovider","title":"OrangeThemeInputTextComponentTokensProvider","type":"topic","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeInputTextComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeInputTextComponentTokensProvider","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeInputTextComponentTokensProvider"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeInputTextComponentTokensProvider/inputTextColorContentDefaultPressed":{"abstract":[{"type":"text","text":"Has for value  colors.colorContentDefault"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeInputTextComponentTokensProvider\/inputTextColorContentDefaultPressed","url":"\/documentation\/oudsthemesorange\/orangethemeinputtextcomponenttokensprovider\/inputtextcolorcontentdefaultpressed","role":"symbol","type":"topic","title":"inputTextColorContentDefaultPressed","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"inputTextColorContentDefaultPressed"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"MultipleColorSemanticTokens","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens"}],"kind":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"title":"OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","kind":"symbol","role":"collec