eInputTextComponentTokensProvider":{"type":"topic","title":"OrangeThemeInputTextComponentTokensProvider","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeInputTextComponentTokensProvider"}],"role":"symbol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeInputTextComponentTokensProvider"}],"url":"\/documentation\/oudsthemesorange\/orangethemeinputtextcomponenttokensprovider","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens of input text","type":"text"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"input text","type":"text"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSInputText"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"code":"AllInputTextComponentTokensProvider","type":"codeVoice"},{"text":" implementation inside ","type":"text"},{"code":"OUDSTheme","type":"codeVoice"},{"text":" so as to provide","type":"text"},{"text":" ","type":"text"},{"text":"all tokens to the users. It helps users to override some of the tokens and assign them to an ","type":"text"},{"code":"OUDSTheme","type":"codeVoice"},{"text":" implementation to use.","type":"text"},{"text":" ","type":"text"},{"text":"Custom themes can use subclass of ","type":"text"},{"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeInputTextComponentTokensProvider","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"InouttextComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"input text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Inout text","type":"text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), colors ("},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeInputTextComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeInputTextComponentTokensProvider/inputTextSpacePaddingInlineContainerStart":{"role":"symbol","type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeInputTextComponentTokensProvider\/inputTextSpacePaddingInlineContainerStart","title":"inputTextSpacePaddingInlineContainerStart","url":"\/documentation\/oudsthemesorange\/orangethemeinputtextcomponenttokensprovider\/inputtextspacepaddinginlinecontainerstart","abstract":[{"text":"Has for value token spaces.spacePaddingInlineMd","type":"text"}],"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"inputTextSpacePaddingInlineContainerStart","kind":"identifier"},{"text":": ","kind":"text"},{"text":"SpaceSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","kind":"typeIdentifier"}],"kind":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","title":"OUDSThemesOrange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"kind":"symbol","type":"topic","role":"collection","url":"\/documentation\/oudsthemesorange"}}}