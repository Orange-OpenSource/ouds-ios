ontent":[{"type":"text","text":"input text"}]},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Inout text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"), colors ("},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeInputTextComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemeinputtextcomponenttokensprovider","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"kind":"symbol","role":"collection","title":"OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeInputTextComponentTokensProvider/inputTextColorBgDefaultLoading":{"abstract":[{"text":"Has for value token colors.colorActionSupportLoading","type":"text"}],"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"inputTextColorBgDefaultLoading","kind":"identifier"},{"text":": ","kind":"text"},{"text":"MultipleColorSemanticTokens","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens","kind":"typeIdentifier"}],"url":"\/documentation\/oudsthemes