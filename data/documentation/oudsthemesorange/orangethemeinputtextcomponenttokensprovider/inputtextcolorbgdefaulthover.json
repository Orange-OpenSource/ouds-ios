":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Inout text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"), colors ("},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeInputTextComponentTokensProvider"}],"type":"topic","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeInputTextComponentTokensProvider"}],"title":"OrangeThemeInputTextComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeInputTextComponentTokensProvider/inputTextColorBgDefaultHover":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeInputTextComponentTokensProvider\/inputTextColorBgDefaultHover","kind":"symbol","role":"symbol","title":"inputTextColorBgDefaultHover","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"inputTextColorBgDefaultHover"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"MultipleColorSemanticTokens","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens"}],"abstract":[{"text":"Has for value  colors.colorActionSupportHover","type":"text"}],"url":"\/documentation\/oudsthemesorange\/orangethemeinputtextcomponenttokensprovider\/inputtextcolorbgdefaulthover","type":"topic"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"type":"topic","role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{