agments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeInputTextComponentTokensProvider"}],"navigatorTitle":[{"kind":"identifier","text":"OrangeThemeInputTextComponentTokensProvider"}],"type":"topic","kind":"symbol","title":"OrangeThemeInputTextComponentTokensProvider","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens of input text","type":"text"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"input text","type":"text"}]},{"type":"text","text":" objects like "},{"code":"OUDSInputText","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"code":"AllInputTextComponentTokensProvider","type":"codeVoice"},{"type":"text","text":" implementation inside "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeInputTextComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"code":"InouttextComponentTokens","type":"codeVoice"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"input text"}]},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Inout text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"), colors ("},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeInputTextComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemeinputtextcomponenttokensprovider","role":"symbol"}}}