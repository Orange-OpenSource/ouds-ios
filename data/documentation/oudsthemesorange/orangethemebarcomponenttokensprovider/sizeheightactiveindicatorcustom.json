 "},{"type":"emphasis","inlineContent":[{"type":"text","text":"bar"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"inlineContent":[{"type":"text","text":"Bar"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with semantic tokens of colors ("},{"type":"codeVoice","code":"AllColorSemanticTokensProviders"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and effects (from "},{"type":"codeVoice","code":"AllEffectSemanticTokensProvider"},{"type":"text","text":")."}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeBarComponentTokensProvider/sizeHeightActiveIndicatorCustom":{"kind":"symbol","type":"topic","title":"sizeHeightActiveIndicatorCustom","role":"symbol","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"sizeHeightActiveIndicatorCustom","kind":"identifier"},{"text":": ","kind":"text"},{"text":"SizeSemanticToken","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBarComponentTokensProvider\/sizeHeightActiveIndicatorCustom","url":"\/documentation\/oudsthemesorange\/orangethemebarcomponenttokensprovider\/sizeheightactiveindicatorcustom","abstract":[{"type":"text","text":"Has for value token DimensionRawTokens.outOfSystem75"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","images":[{"type":"icon","identifier":"ic_theme_orange.png"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","type":"topic","kind":"symbol","abstract":[{"text":"The Orange theme overrides some tokens from the basic ","type":"text"},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"title":"Theme Orange","url":"\/documentation\/oudsthemesorange"}}}