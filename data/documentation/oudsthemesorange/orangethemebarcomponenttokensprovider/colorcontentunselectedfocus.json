type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and effects (from "},{"type":"codeVoice","code":"AllEffectSemanticTokensProvider"},{"type":"text","text":")."}]}}}