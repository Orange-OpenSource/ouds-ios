ough any ","type":"text"},{"type":"codeVoice","code":"OUDSTheme"},{"text":".","type":"text"},{"text":" ","type":"text"},{"inlineContent":[{"type":"text","text":"Bar"}],"type":"emphasis"},{"text":" components tokens are defined with semantic tokens of colors (","type":"text"},{"type":"codeVoice","code":"AllColorSemanticTokensProviders"},{"text":"),","type":"text"},{"text":" ","type":"text"},{"text":"sizes (from ","type":"text"},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"text":"), borders (from ","type":"text"},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"text":")","type":"text"},{"text":" ","type":"text"},{"text":"and effects (from ","type":"text"},{"type":"codeVoice","code":"AllEffectSemanticTokensProvider"},{"text":").","type":"text"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBarComponentTokensProvider","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeBarComponentTokensProvider"}],"kind":"symbol","type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemebarcomponenttokensprovider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeBarComponentTokensProvider/colorActiveIndicatorCustomSelectedPressed":{"title":"colorActiveIndicatorCustomSelectedPressed","role":"symbol","abstract":[{"text":"Has for value token colors.actionPressed","type":"text"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBarComponentTokensProvider\/colorActiveIndicatorCustomSelectedPressed","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"colorActiveIndicatorCustomSelectedPressed","kind":"identifier"},{"text":": ","kind":"text"},{"text":"MultipleColorSemanticToken","kind":"typeIdentifier","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticToken"}],"kind":"symbol","type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemebarcom