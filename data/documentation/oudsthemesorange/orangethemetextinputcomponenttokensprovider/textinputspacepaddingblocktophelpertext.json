text":"), spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and  dimensions (from "},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")."}],"role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeTextInputComponentTokensProvider","kind":"identifier"}]}}}