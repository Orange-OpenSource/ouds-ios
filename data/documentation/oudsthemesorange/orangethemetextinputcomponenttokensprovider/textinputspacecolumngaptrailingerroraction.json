meTextInputComponentTokensProvider/textInputSpaceColumnGapTrailingErrorAction":{"type":"topic","title":"textInputSpaceColumnGapTrailingErrorAction","abstract":[{"type":"text","text":"Has for value token spaces.spaceColumnGapXs"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider\/textInputSpaceColumnGapTrailingErrorAction","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"textInputSpaceColumnGapTrailingErrorAction","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","text":"SpaceSemanticToken","kind":"typeIdentifier"}],"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemetextinputcomponenttokensprovider\/textinputspacecolumngaptrailingerroraction","role":"symbol"},"ic_theme_orange.png":{"identifier":"ic_theme_orange.png","variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png","traits":["1x","light"]}],"type":"image","alt":null},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTextInputComponentTokensProvider":{"kind":"symbol","type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemetextinputcomponenttokensprovider","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","navigatorTitle":[{"text":"OrangeThemeTextInputComponentTokensProvider","kind":"identifier"}],"abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens of input text","type":"text"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"input text","type":"text"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSTextInput"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllTextInputComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"TextInputComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"input text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Inout text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of colors ("},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"), spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and  dimensions (from "},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")."}],"title":"OrangeThemeTextInputComponentTokensProvider","role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeTextInputComponentTokensProvider","kind":"identifier"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"images":[{"type":"icon","identifier":"ic_theme_orange.png"}],"title":"OUDSThemesOrange","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","role":"collection","url":"\/documentation\/oudsthemesorange"}}}