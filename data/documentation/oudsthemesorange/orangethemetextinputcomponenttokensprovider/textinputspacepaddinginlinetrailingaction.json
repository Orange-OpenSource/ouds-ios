eTextInputComponentTokensProvider/textInputSpacePaddingInlineTrailingAction":{"fragments":[{"text":"var","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"textInputSpacePaddingInlineTrailingAction"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","text":"SpaceSemanticToken"}],"title":"textInputSpacePaddingInlineTrailingAction","abstract":[{"type":"text","text":"Has for value token spaces.spacePaddingInline2xs"}],"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemetextinputcomponenttokensprovider\/textinputspacepaddinginlinetrailingaction","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider\/textInputSpacePaddingInlineTrailingAction","type":"topic","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","url":"\/documentation\/oudsthemesorange","role":"collection","title":"OUDSThemesOrange","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTextInputComponentTokensProvider":{"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemetextinputcomponenttokensprovider","type":"topic","title":"OrangeThemeTextInputComponentTokensProvider","navigatorTitle":[{"text":"OrangeThemeTextInputComponentTokensProvider","kind":"identifier"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of input text"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"input text"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSTextInput"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllTextInputComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"TextInputComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"text":"input text","type":"text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Inout text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of colors ("},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"), spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and  dimensions (from "},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")."}],"role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeTextInputComponentTokensProvider","kind":"identifier"}]}}}