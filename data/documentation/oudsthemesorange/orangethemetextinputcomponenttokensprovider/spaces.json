om "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and  dimensions (from "},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTextInputComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","type":"topic"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"type":"topic","images":[{"identifier":"ic_theme_orange.svg","type":"card"},{"type":"icon","identifier":"ic_theme_orange.svg"}],"title":"OUDSThemesOrange","role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"url":"\/documentation\/oudsthemesorange","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"},"ic_theme_orange.svg":{"variants":[{"traits":["1x","light"],"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.svg"}],"type":"image","identifier":"ic_theme_orange.svg","alt":null}}}