ensProvider"},{"type":"text","text":")."}],"navigatorTitle":[{"text":"OrangeThemeTextInputComponentTokensProvider","kind":"identifier"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemetextinputcomponenttokensprovider"},"doc://OUDS/documentation/OUDS/AllSpaceSemanticTokensProvider":{"url":"\/documentation\/ouds\/allspacesemantictokensprovider","type":"topic","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"Something which provides all semantic tokens of space"}],"title":"AllSpaceSemanticTokensProvider","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AllSpaceSemanticTokensProvider","kind":"identifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:4OUDS14TokensProviderP","text":"TokensProvider","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic013SpaceMultipleB6TokensP","text":"SpaceMultipleSemanticTokens","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB6TokensP","text":"SpaceSemanticTokens","kind":"typeIdentifier"}],"identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllSpaceSemanticTokensProvider"}}}