":"text","text":" "},{"inlineContent":[{"type":"text","text":"Inout text"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with raw and semantic tokens of colors ("},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"), spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and  dimensions (from "},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")."}],"navigatorTitle":[{"text":"OrangeThemeTextInputComponentTokensProvider","kind":"identifier"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemetextinputcomponenttokensprovider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTextInputComponentTokensProvider/textInputColorBorderEnabled":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider\/textInputColorBorderEnabled","abstract":[{"type":"text","text":"Has for value token colors.colorBorderDefault"}],"title":"textInputColorBorderEnabled","role":"symbol","kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemetextinputcomponenttokensprovider\/textinputcolorborderenabled","type":"topic","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"textInputColorBorderEnabled"},{"kind":"text","text":": "},{"preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens","kind":"typeIdentifier","text":"MultipleColorSemanticTokens"}]}}}