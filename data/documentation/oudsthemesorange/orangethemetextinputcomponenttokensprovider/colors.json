ens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"input text"}]},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Inout text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of colors ("},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"code":"AllBorderSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"), spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and  dimensions (from "},{"code":"AllDimensionSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemetextinputcomponenttokensprovider","type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTextInputComponentTokensProvider"}],"title":"OrangeThemeTextInputComponentTokensProvider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTextInputComponentTokensProvider"}],"role":"symbol"}}}