xt":" "},{"type":"emphasis","inlineContent":[{"text":"Button","type":"text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeButtonComponentTokensProvider","type":"topic","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeButtonComponentTokensProvider","kind":"identifier"}],"url":"\/documentation\/oudsthemesorange\/orangethemebuttoncomponenttokensprovider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeButtonComponentTokensProvider/buttonColorContentStrongHoverMono":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeButtonComponentTokensProvider\/buttonColorContentStrongHoverMono","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"buttonColorContentStrongHoverMono","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens","text":"MultipleColorSemanticTokens","kind":"typeIdentifier"}],"role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemebuttoncomponenttokensprovider\/buttoncolorcontentstronghovermono","kind":"symbol","type":"topic","abstract":[{"text":"Has for value  colors.colorRepositoryNeutralMutedWhite","type":"text"}],"title":"buttonColorContentStrongHoverMono"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"type":"topic","role":"collection","title":"OUDSThemesOrange","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}]}}}