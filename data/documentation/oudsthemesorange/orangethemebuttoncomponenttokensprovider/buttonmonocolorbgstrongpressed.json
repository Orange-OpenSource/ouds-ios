buttons"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Button","type":"text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"role":"symbol"}}}