text","text":"buttons"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Button"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), borders"},{"text":" ","type":"text"},{"text":"spaces (from ","type":"text"},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"text":").","type":"text"}],"role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeButtonComponentTokensProvider/buttonSizeMinWidth":{"url":"\/documentation\/oudsthemesorange\/orangethemebuttoncomponenttokensprovider\/buttonsizeminwidth","role":"symbol","abstract":[{"text":"Has for value token sizes.sizeMinInteractiveArea","type":"text"}],"kind":"symbol","type":"topic","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"buttonSizeMinWidth","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena","text":"SizeSemanticToken","kind":"typeIdentifier"}],"title":"buttonSizeMinWidth","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeButtonComponentTokensProvider\/buttonSizeMinWidth"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","url":"\/documentation\/oudsthemesorange","role":"collection","title":"OUDSThemesOrange","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"}}}