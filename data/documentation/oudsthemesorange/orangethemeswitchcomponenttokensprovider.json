 class CustomSwitchComponentTokensProvider: OrangeThemeSwitchComponentTokensProvider {","","        \/\/ Then override the switch component tokens you want.","","        override var switchColorCursor: MultipleColorSemanticTokens { colors.colorRepositoryPositiveDefault }","","        override var switchSpacePaddingInlineUnselected: SpaceSemanticToken { spaces.spaceFixedShortest }","","        override var switchSize: SizeSemanticToken { sizes.sizeIconWithLabelLargeSizeXl }","        \/\/ ...","    }","","    \/\/ Or define your own provider from scratch","    class CustomSwitchComponentTokensProvider: SwitchComponentTokens {","","        \/\/ And implement maybe hundreds of tokens.","        \/\/ You are allowed to use semantic tokens providers if you want to define values.","    }"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"text":"Then, you can give this ","type":"text"},{"code":"CustomSwitchComponentTokensProvider","type":"codeVoice"},{"text":" to your own theme implementation:","type":"text"}],"type":"paragraph"},{"code":["class LocalTheme: OrangeTheme {","","    override init() {","        super.init(switch: CustomSwitchComponentTokensProvider())","    }","}"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"text":"or to an already existing theme for example:","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["    OrangeTheme(switch: CustomSwitchComponentTokensProvider())"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"It is also possible to use your own semantic tokens providers for this component tokens providers:"}]},{"syntax":"swift","type":"codeListing","code":["    \/\/ Uses by default here:","    \/\/ - OrangeThemeSizeSemanticTokensProvider for sizes","    \/\/ - OrangeThemeColorSemanticTokensProvider for colors","    \/\/ - OrangeThemeSpaceSemanticTokensProvider for spaces","    \/\/ - OrangeThemeOpacitySemanticTokensProvider for opacities","    \/\/ - OrangeThemeBorderSemanticTokensProvider for borders","    let switchComponentTokensProvider = OrangeThemeSwitchComponentTokensProvider()","","    \/\/ Or use your own size, color, border, space and opacities semantic tokens providers (or only some)","    let switchComponentTokensProvider = OrangeThemeSwitchComponentTokensProvider(","                                                sizes: CustomSizeSemanticTokensProvider(),","                                                borders: CustomBorderSemanticTokensProvider(),","                                                colors: CustomColorSemanticTokensProvider(),","                                                space: CustomSpaceSemanticTokensProvider(),","                                                opacities: CustomOpacitySemanticTokensProvider())"]},{"style":"note","name":"Since","type":"aside","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"0.9.0"}]}]}]}],"identifier":{"url":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider","interfaceLanguage":"swift"},"topicSections":[{"generated":true,"identifiers":["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/init(sizes:borders:colors:spaces:opacities:)"],"anchor":"Initializers","title":"Initializers"},{"generated":true,"identifiers":["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/borders","doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/colors","doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/opacities","doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/sizes","doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider\/spaces"],"anchor":"Instance-Properties","title":"Instance Properties"},{"generated":true,"identifiers":["