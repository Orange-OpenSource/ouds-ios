text"},{"code":"SwitchComponentTokens","type":"codeVoice"},{"text":" so as to expose the component tokens for ","type":"text"},{"inlineContent":[{"text":"switch \/ toggle","type":"text"}],"type":"emphasis"},{"text":" through any ","type":"text"},{"code":"OUDSTheme","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"inlineContent":[{"type":"text","text":"Switch"}],"type":"emphasis"},{"text":" components tokens are defined with semantic tokens of colors (from ","type":"text"},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"text":"),","type":"text"},{"text":" ","type":"text"},{"text":"spaces (from ","type":"text"},{"code":"AllSpacesSemanticTokensProvider","type":"codeVoice"},{"text":") ,  dimensions (","type":"text"},{"code":"AllDimensionSemanticTokensProvider","type":"codeVoice"},{"text":"),","type":"text"},{"text":" ","type":"text"},{"text":"sizes (","type":"text"},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"text":"), borders (","type":"text"},{"code":"AllBorderSemanticTokensProvider","type":"codeVoice"},{"text":") and opacities (","type":"text"},{"code":"AllOpacitiySemanticTokensProvider","type":"codeVoice"},{"text":").","type":"text"}],"relationshipsSections":[{"identifiers":["doc:\/\/OUDS\/documentation\/OUDS\/AllSwitchComponentTokensProvider","doc:\/\/OUDSThemesOrange\/19OUDSTokensComponent06SwitchB6TokensP","doc:\/\/OUDSThemesOrange\/s8CopyableP","doc:\/\/OUDS\/documentation\/OUDS\/TokensProvider"],"title":"Conforms To","kind":"relationships","type":"conformsTo"}],"identifier":{"url":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeSwitchComponentTokensProvider","interfaceLanguage":"swift"},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/oudsthemesorange\/orangethemeswitchcomponenttokensprovider"]}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeSwi