s of dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProviders"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBadgeComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemebadgecomponenttokensprovider","role":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeBadgeComponentTokensProvider"}],"title":"OrangeThemeBadgeComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeBadgeComponentTokensProvider/badgeSpacePaddingInlineMedium":{"role":"symbol","kind":"symbol","type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBadgeComponentTokensProvider\/badgeSpacePaddingInlineMedium","title":"badgeSpacePaddingInlineMedium","url":"\/documentation\/oudsthemesorange\/orangethemebadgecomponenttokensprovider\/badgespacepaddinginlinemedium","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"badgeSpacePaddingInlineMedium"},{"kind":"text","text":": "},{"preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","kind":"typeIdentifier","text":"SpaceSemanticToken"}],"abstract":[{"text":"Has for value token spaces.spacePaddingInline3xs","type":"text"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"url":"\/documentation\/oudsthemesorange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","title":"OUDSThemesOrange"}}}