bol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeBadgeComponentTokensProvider"}],"navigatorTitle":[{"kind":"identifier","text":"OrangeThemeBadgeComponentTokensProvider"}],"abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of badge"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"badge","type":"text"}]},{"type":"text","text":" objects like "},{"code":"OUDSBadge","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"code":"AllBadgeComponentTokensProvider","type":"codeVoice"},{"type":"text","text":" implementation inside "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBadgeComponentTokensProvider","type":"reference","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"code":"BadgeComponentTokens","type":"codeVoice"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"badge"}]},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Badge","type":"text"}]},{"type":"text","text":" components tokens are defined with semantic tokens of dimensions ("},{"code":"AllDimensionSemanticTokensProviders","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"url":"\/documentation\/oudsthemesorange\/orangethemebadgecomponenttokensprovider","kind":"symbol","title":"OrangeThemeBadgeComponentTokensProvider","type":"topic"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeBadgeComponentTokensProvider/badgeSpaceInset":{"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBadgeComponentTokensProvider\/badgeSpaceInset","url":"\/documentation\/oudsthemesorange\/orangethemebadgecomponenttokensprovider\/badgespaceinset","kind":"symbol","role":"symbol","title":"badgeSpaceInset","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"badgeSpaceInset","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","text":"SpaceSemanticToken","kind":"typeIdentifier"}],"abstract":[{"type":"text","text":"Has for value token DimensionRawTokens.dimensionOutOfSystem75"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","url":"\/documentation\/oudsthemesorange","role":"collection","title":"OUDSThemesOrange","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"}}}