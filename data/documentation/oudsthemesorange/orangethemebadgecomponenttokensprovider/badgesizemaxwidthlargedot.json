heme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Badge","type":"text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of dimensions ("},{"type":"codeVoice","code":"DimensionRawToken"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":")."}],"title":"OrangeThemeBadgeComponentTokensProvider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeBadgeComponentTokensProvider"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeBadgeComponentTokensProvider/badgeSizeMaxWidthLargeDot":{"kind":"symbol","abstract":[{"text":"Has for value token DimensionRawTokens.dimension250","type":"text"}],"role":"symbol","title":"badgeSizeMaxWidthLargeDot","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"badgeSizeMaxWidthLargeDot","kind":"identifier"},{"text":": ","kind":"text"},{"text":"SizeSemanticToken","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena"}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeBadgeComponentTokensProvider\/badgeSizeMaxWidthLargeDot","url":"\/documentation\/oudsthemesorange\/orangethemebadgecomponenttokensprovider\/badgesizemaxwidthlargedot"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"type":"topic","role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"kind":"symbol","url":"\/documentation\/oudsthemesorange","title":"OUDSThemesOrange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"}}}