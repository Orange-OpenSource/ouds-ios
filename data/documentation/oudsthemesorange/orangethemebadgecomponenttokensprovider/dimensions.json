 expose the component tokens for "},{"type":"emphasis","inlineContent":[{"text":"badge","type":"text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Badge"}]},{"type":"text","text":" components tokens are defined with semantic tokens of dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProviders"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"navigatorTitle":[{"text":"OrangeThemeBadgeComponentTokensProvider","kind":"identifier"}]}}}