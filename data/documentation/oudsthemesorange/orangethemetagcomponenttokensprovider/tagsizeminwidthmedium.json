abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens of tag","type":"text"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"tag"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSTag"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllTagComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider","isActive":true,"type":"reference"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"TagComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"tags"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Tags"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"), spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":") and borders"},{"type":"text","text":" "},{"type":"text","text":"(from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTagComponentTokensProvider"}],"title":"OrangeThemeTagComponentTokensProvider","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTagComponentTokensProvider"}],"url":"\/documentation\/oudsthemesorange\/orangethemetagcomponenttokensprovider","role":"symbol","type":"topic"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"title":"OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","kind":"symbol","role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTagComponentTokensProvider/tagSizeMinWidthMedium":{"fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"tagSizeMinWidthMedium"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"SizeSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena"}],"title":"tagSizeMinWidthMedium","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider\/tagSizeMinWidthMedium","url":"\/documentation\/oudsthemesorange\/orangethemetagcomponenttokensprovider\/tagsizeminwidthmedium","rol