l "},{"type":"codeVoice","code":"TagComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"inlineContent":[{"text":"tags","type":"text"}],"type":"emphasis"},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"inlineContent":[{"text":"Tags","type":"text"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":") , spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"type":"topic","role":"symbol","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider","title":"OrangeThemeTagComponentTokensProvider","navigatorTitle":[{"text":"OrangeThemeTagComponentTokensProvider","kind":"identifier"}],"url":"\/documentation\/oudsthemesorange\/orangethemetagcomponenttokensprovider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTagComponentTokensProvider/tagSpacePaddingInlineDefault":{"role":"symbol","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"tagSpacePaddingInlineDefault"},{"kind":"text","text":": "},{"preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","kind":"typeIdentifier","text":"SpaceSemanticToken"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider\/tagSpacePaddingInlineDefault","abstract":[{"text":"Has for value token spaces.spacePaddingInlineMd","