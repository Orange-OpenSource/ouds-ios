on/OUDSThemesOrange/OrangeThemeTagComponentTokensProvider/tagSizeMinWidthDefault":{"title":"tagSizeMinWidthDefault","url":"\/documentation\/oudsthemesorange\/orangethemetagcomponenttokensprovider\/tagsizeminwidthdefault","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"tagSizeMinWidthDefault"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena","text":"SizeSemanticToken"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider\/tagSizeMinWidthDefault","kind":"symbol","type":"topic","role":"symbol","abstract":[{"text":"Has for value token dimensions.dimensionXl","type":"text"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTagComponentTokensProvider":{"role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemetagcomponenttokensprovider","type":"topic","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTagComponentTokensProvider"}],"title":"OrangeThemeTagComponentTokensProvider","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTagComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider","kind":"symbol","abstract":[{"type":"text","text":"A class which wraps all "},{"inlineContent":[{"type":"text","text":"component  tokens of tag"}],"type":"strong"},{"type":"text","text":" for "},{"inlineContent":[{"type":"text","text":"tag"}],"type":"emphasis"},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSTag"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllTagComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"TagComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"inlineContent":[{"text":"tags","type":"text"}],"type":"emphasis"},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"inlineContent":[{"text":"Tags","type":"text"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with semantic tokens of dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":") ,"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":") , sizes (from "},{"type":"codeVoice","code":"AllSizesSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"images":[{"type":"icon","identifier":"ic_theme_orange.png"}],"kind":"symbol","type":"topic","title":"Theme Orange","url":"\/documentation\/oudsthemesorange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"}}}