type":"emphasis"},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"inlineContent":[{"text":"Tags","type":"text"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with semantic tokens of dimensions ("},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":") ,"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":") , sizes (from "},{"type":"codeVoice","code":"AllSizesSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTagComponentTokensProvider/tagSpaceInsetIconDefault":{"type":"topic","title":"tagSpaceInsetIconDefault","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider\/tagSpaceInsetIconDefault","kind":"symbol","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"tagSpaceInsetIconDefault","kind":"identifier"},{"text":": ","kind":"text"},{"text":"SpaceSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","kind":"typeIdentifier"}],"role":"symbol","abstract":[{"text":"Has for value token spaces.spaceInset4xs","type":"text"}],"url":"\/documentation\/oudsthemesorange\/orangethemetagcomponenttokensprovider\/tagspaceinseticondefault"},"ic_theme_orange.png":{"identifier":"ic_theme_orange.png","alt":null,"variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png","traits":["1x","light"]}],"type":"image"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":