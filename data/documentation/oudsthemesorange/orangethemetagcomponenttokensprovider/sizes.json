paces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":") and borders"},{"type":"text","text":" "},{"type":"text","text":"(from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTagComponentTokensProvider"}],"title":"OrangeThemeTagComponentTokensProvider","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTagComponentTokensProvider"}],"url":"\/documentation\/oudsthemesorange\/orangethemetagcomponenttokensprovider","role":"symbol","type":"topic"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTagComponentTokensProvider/sizes":{"abstract":[{"type":"text","text":"Provider of size semantic tokens to use for tag suzes"}],"role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemetagcomponenttokensprovider\/sizes","kind":"symbol","fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"sizes","kind":"identifier"},{"text":": any ","kind":"text"},{"text":"AllSizeSemanticTokensProvider","kind":"typeIdentifier","preciseIdentifier":"s:4OUDS29AllSizeSemanticTokensProviderP"}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagComponentTokensProvider\/sizes","title":"sizes"},"doc://OUDS/documentation/OUDS/AllSizeSemanticTokensProvider":{"identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllSizeSemanticTokensProvider","abstract":[{"text":"Something which provides all semantic tokens of size","type":"text"}],"title":"AllSizeSemanticTokensProvider","url":"\/documentation\/ouds\/allsizesemantictokensprovider","kind":"symbol","type":"topic","role":"symbol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"AllSizeSemanticTokensProvider"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","preciseIdentifier"