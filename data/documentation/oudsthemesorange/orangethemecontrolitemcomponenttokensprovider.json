{"identifier":{"url":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","interfaceLanguage":"swift"},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider"]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"metadata":{"modules":[{"name":"Theme Orange"}],"role":"symbol","title":"OrangeThemeControlItemComponentTokensProvider","externalID":"s:16OUDSThemesOrange0B39ThemeControlItemComponentTokensProviderC","roleHeading":"Class","symbolKind":"class","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"navigatorTitle":[{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}]},"relationshipsSections":[{"kind":"relationships","title":"Conforms To","type":"conformsTo","identifiers":["doc:\/\/OUDS\/documentation\/OUDS\/AllControlItemComponentTokensProvider","doc:\/\/OUDSThemesOrange\/19OUDSTokensComponent011ControlItemB6TokensP","doc:\/\/OUDSThemesOrange\/s8CopyableP","doc:\/\/OUDS\/documentation\/OUDS\/TokensProvider"]}],"abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens","type":"text"}]},{"type":"text","text":" for components with a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"control item"}]},{"type":"text","text":" layout like "},{"code":"OUDSSwitch","type":"codeVoice"},{"type":"text","text":", "},{"code":"OUDSRadioButton","type":"codeVoice"},{"type":"text","text":" and "},{"code":"OUDSCheckboItem","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"code":"AllControlItemComponentTokensProvider","type":"codeVoice"},{"type":"text","text":" implementation inside "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"code":"ControlItemComponentTokens","type":"codeVoice"},{"type":"text","text":" so as to expose the component tokens for control-item-layout-basded components through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Such components tokens are defined with raw and semantic tokens of sizes (from "},{"code":"SizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"code":"AllBorderSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),  colors (from "},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")  and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"platforms":["macOS"],"languages":["swift"]}]},{"kind":"content","content":[{"text":"Overview","anchor":"overview","level":2,"type":"heading"},{"code":["    \/\/ Define your own provider for control-item-layout-based component tokens","    \/\/ by inheriting from existing provider","    class CustomControlItemComponentTokensProvider: OrangeThemeControlItemComponentTokensProvider {","","        \/\/ Then override the control-item-layout-based component tokens you want.","","        override var controlItemSizeMinHeight: SizeSemanticToken { DimensionRawTokens.dimension500 }","","        override var controlItemColorBgFocus: MultipleColorSemanticTokens { colors.colorContentDefault }","","        override var controlItemSpaceRowGap: SpaceSemanticToken { spaces.spacePaddingInlineTall }","","        \/\/ ...","    }","","    \/\/ Or define your own provider from scratch","    class CustomControlItemComponentTokensProvider: ControlItemComponentTokens {","","        \/\/ And implement hundreds of tokens.","        \/\/ You are allowed to give semantic tokens providers if you want to define values.","    }"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Then, you can give this "},{"code":"CustomControlItemComponentTokensProvider","type":"codeVoice"},{"type":"text","text":" to your own theme implementation:"}]},{"code":["class LocalTheme: OrangeTheme {","","    override init() {","        super.init(controlItem: CustomControlItemComponentTokensProvider())","    }","}"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"or to an already existing theme for example:"}]},{"code":["    OrangeTheme(controlItem: CustomControlItemComponentTokensProvider())"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"It is also possible to use your own semantic tokens providers for this component tokens providers:"}]},{"code":["    \/\/ Uses by default here:","    \/\/ - OrangeThemeSizeSemanticTokensProvider for sizes","    \/\/ - OrangeThemeBorderSemanticTokensProvider for borders","    \/\/ - OrangeThemeColorSemanticTokensProvider for colors","    \/\/ - OrangeThemeSpaceSemanticTokensProvider for spaces","    let controlItemComponentTokensProvider = OrangeThemeControlItemComponentTokensProvider()","","    \/\/ Or use your own size, color and space semantic tokens providers (or only some)","    let controlItemComponentTokensProvider = OrangeThemeControlItemComponentTokensProvider(","                                                sizes: CustomSizeSemanticTokensProvider(),","                                                borders: CustomBorderSemanticTokensProvider(),","                                                colors: CustomColorSemanticTokensProvider(),","                                                space: CustomSpaceSemanticTokensProvider())"],"syntax":"swift","type":"codeListing"},{"style":"note","name":"Since","type":"aside","content":[{"type":"paragraph","inlineContent":[{"text":"0.10.0","type":"text"}]}]}]}],"topicSections":[{"generated":true,"title":"Initializers","anchor":"Initializers","identifiers":["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/init(sizes:borders:colors:spaces:)"]},{"generated":true,"title":"Instance Properties","anchor":"Instance-Properties","identifiers":["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/borders","doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/colors","doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/sizes","doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/spaces"]},{"generated":true,"title":"Default Implementations","anchor":"Default-Implementations","identifiers":["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/ControlItemComponentTokens-Implementations","do