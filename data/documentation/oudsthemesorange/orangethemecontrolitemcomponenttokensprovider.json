"syntax":"swift","code":["    OrangeTheme(controlItem: CustomControlItemComponentTokensProvider())"]},{"inlineContent":[{"text":"It is also possible to use your own semantic tokens providers for this component tokens providers:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["    \/\/ Uses by default here:","    \/\/ - OrangeThemeSizeSemanticTokensProvider for sizes","    \/\/ - OrangeThemeBorderSemanticTokensProvider for borders","    \/\/ - OrangeThemeColorSemanticTokensProvider for colors","    \/\/ - OrangeThemeSpaceSemanticTokensProvider for spaces","    let controlItemComponentTokensProvider = OrangeThemeControlItemComponentTokensProvider()","","    \/\/ Or use your own size, color and space semantic tokens providers (or only some)","    let controlItemComponentTokensProvider = OrangeThemeControlItemComponentTokensProvider(","                                                sizes: CustomSizeSemanticTokensProvider(),","                                                borders: CustomBorderSemanticTokensProvider(),","                                                colors: CustomColorSemanticTokensProvider(),","                                                space: CustomSpaceSemanticTokensProvider())"]},{"style":"note","name":"Since","type":"aside","content":[{"inlineContent":[{"text":"0.10.0","type":"text"}],"type":"paragraph"}]}],"kind":"content"}],"identifier":{"url":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","interfaceLanguage":"swift"},"metadata":{"title":"OrangeThemeControlItemComponentTokensProvider","symbolKind":"class","role":"symbol","externalID":"s:16OUDSThemesOrange0B39ThemeControlItemComponentTokensProviderC","roleHeading":"Class","modules":[{"name":"OUDSThemesOrange"}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"navigatorTitle":[{"text":"OrangeTh