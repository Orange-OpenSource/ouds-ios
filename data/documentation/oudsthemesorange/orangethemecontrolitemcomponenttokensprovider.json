","code":["class LocalTheme: OrangeTheme {","","    override init() {","        super.init(controlItem: CustomControlItemComponentTokensProvider())","    }","}"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"or to an already existing theme for example:"}]},{"type":"codeListing","code":["    OrangeTheme(controlItem: CustomControlItemComponentTokensProvider())"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"It is also possible to use your own semantic tokens providers for this component tokens providers:"}]},{"type":"codeListing","code":["    \/\/ Uses by default here:","    \/\/ - OrangeThemeSizeSemanticTokensProvider for sizes","    \/\/ - OrangeThemeBorderSemanticTokensProvider for borders","    \/\/ - OrangeThemeColorSemanticTokensProvider for colors","    \/\/ - OrangeThemeSpaceSemanticTokensProvider for spaces","    let controlItemComponentTokensProvider = OrangeThemeControlItemComponentTokensProvider()","","    \/\/ Or use your own size, color and space semantic tokens providers (or only some)","    let controlItemComponentTokensProvider = OrangeThemeControlItemComponentTokensProvider(","                                                sizes: CustomSizeSemanticTokensProvider(),","                                                borders: CustomBorderSemanticTokensProvider(),","                                                colors: CustomColorSemanticTokensProvider(),","                                                space: CustomSpaceSemanticTokensProvider())"],"syntax":"swift"},{"style":"note","content":[{"type":"paragraph","inlineContent":[{"text":"0.10.0","type":"text"}]}],"type":"aside","name":"Since"}],"kind":"content"}],"abstract":[{"type":"text","text":"A class which wraps all "},{"inlineContent":[{"type":"text","text":"component  tokens"}],"type":"strong"},{"type":"text","text":" for components with a "},{"inlineContent":[{"text":"control item","type":"text"}],"type":"emphasis"},{"type":"text