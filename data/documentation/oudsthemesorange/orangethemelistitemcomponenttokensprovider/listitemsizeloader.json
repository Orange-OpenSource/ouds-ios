umentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeListItemComponentTokensProvider"}],"abstract":[{"type":"text","text":"A class which wraps all "},{"inlineContent":[{"type":"text","text":"component  tokens of list item"}],"type":"strong"},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"list item"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSListItem"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"text":"This provider should be integrated as a ","type":"text"},{"code":"AllListItemComponentTokensProvider","type":"codeVoice"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"ListItemComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"list item"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"inlineContent":[{"type":"text","text":"List item"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpacesSemanticTokensProvider"},{"type":"text","text":") and sizes ("},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":")."}],"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemelistitemcomponenttokensprovider","type":"topic","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeListItemComponentTokensProvider"}],"role":"symbol","title":"OrangeThemeListItemComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeListItemComponentTokensProvider/listItemSizeLoader":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider\/listItemSizeLoader","title":"listItemSizeLoader","abstract":[],"url":"\/documentation\/oudsthemesorange\/orangethemelistitemcomponenttokensprovider\/listitemsizeloader","kind":"symbol","type":"topic","role":"symbol","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"listItemSizeLoader"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"SizeSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"title":"OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","kind":"symbol","role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/O