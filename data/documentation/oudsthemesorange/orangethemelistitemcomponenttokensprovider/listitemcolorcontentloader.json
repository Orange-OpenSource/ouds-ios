ce"},{"text":" implementation to use.","type":"text"},{"text":" ","type":"text"},{"text":"Custom themes can use subclass of ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider","type":"reference"},{"text":" and apply the provider they need.","type":"text"},{"text":" ","type":"text"},{"text":"It implements also the protocol ","type":"text"},{"code":"ListItemComponentTokens","type":"codeVoice"},{"text":" so as to expose the component tokens for ","type":"text"},{"inlineContent":[{"text":"list item","type":"text"}],"type":"emphasis"},{"text":" through any ","type":"text"},{"code":"OUDSTheme","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"inlineContent":[{"text":"List item","type":"text"}],"type":"emphasis"},{"text":" components tokens are defined with semantic tokens of colors (from ","type":"text"},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"text":"),","type":"text"},{"text":" ","type":"text"},{"text":"spaces (from ","type":"text"},{"code":"AllSpacesSemanticTokensProvider","type":"codeVoice"},{"text":") and sizes (","type":"text"},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"text":").","type":"text"}],"url":"\/documentation\/oudsthemesorange\/orangethemelistitemcomponenttokensprovider","role":"symbol","title":"OrangeThemeListItemComponentTokensProvider","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider","type":"topic","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeListItemComponentTokensProvider"}],"kind":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"type":"topic","role":"collection","title":"OUDSThemesOrange","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","abstract":[{"typ