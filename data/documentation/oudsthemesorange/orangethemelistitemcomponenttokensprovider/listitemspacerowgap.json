:"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider\/listItemSpaceRowGap","abstract":[],"fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"listItemSpaceRowGap"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","text":"SpaceSemanticToken"}],"type":"topic","title":"listItemSpaceRowGap","kind":"symbol","role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemelistitemcomponenttokensprovider\/listitemspacerowgap"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeListItemComponentTokensProvider":{"title":"OrangeThemeListItemComponentTokensProvider","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of list item"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"list item","type":"text"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSListItem"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllListItemComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"ListItemComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"list item"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"List item","type":"text"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpacesSemanticTokensProvider"},{"type":"text","text":") and sizes ("},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeListItemComponentTokensProvider","kind":"identifier"}],"type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemelistitemcomponenttokensprovider","navigatorTitle":[{"text":"OrangeThemeListItemComponentTokensProvider","kind":"identifier"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider","kind":"symbol","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","title":"OUDSThemesOrange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"kind":"symbol","type":"topic","role":"collection","url":"