e":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"list item"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"List item","type":"text"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"spaces (from "},{"type":"codeVoice","code":"AllSpacesSemanticTokensProvider"},{"type":"text","text":") and sizes ("},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeListItemComponentTokensProvider","kind":"identifier"}],"type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemelistitemcomponenttokensprovider","navigatorTitle":[{"text":"OrangeThemeListItemComponentTokensProvider","kind":"identifier"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider","kind":"symbol","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeListItemComponentTokensProvider/listItemColorBgFocus":{"kind":"symbol","title":"listItemColorBgFocus","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"text":"listItemColorBgFocus","kind":"identifier"},{"text":": ","kind":"text"},{"text":"MultipleColorSemanticTokens","kind":"typeIdentifier","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens"}],"abstract":[],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeListItemComponentTokensProvider\/listItemColorBgFocus","type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemelistitemcomponenttokensprovider\/listit