t"},{"inlineContent":[{"text":"Link","type":"text"}],"type":"emphasis"},{"text":" components tokens are defined with raw and semantic tokens of sizes (from ","type":"text"},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"text":"),","type":"text"},{"text":" ","type":"text"},{"text":"colors (from ","type":"text"},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"text":") and spaces (from ","type":"text"},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"text":").","type":"text"}],"navigatorTitle":[{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}],"title":"OrangeThemeLinkComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider/linkSizeIconSmall":{"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"linkSizeIconSmall","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena","text":"SizeSemanticToken","kind":"typeIdentifier"}],"url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider\/linksizeiconsmall","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider\/linkSizeIconSmall","type":"topic","title":"linkSizeIconSmall","abstract":[{"text":"Has for value token sizes.sizeIconWithLabelMediumSizeSm","type":"text"}],"role":"symbol","kind":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","title":"OUDSThemesOrange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"kind":"symbol","type":"topic","role":"collection","url":"\/documentation\/oudsthemesorange"}}}