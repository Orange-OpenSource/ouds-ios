role":"symbol"},"references":{"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider":{"url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider","role":"symbol","type":"topic","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens of links","type":"text"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"text":"link","type":"text"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSLink"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllLinkComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"LinkComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"links"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Link","type":"text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeLinkComponentTokensProvider"}],"title":"OrangeThemeLinkComponentTokensProvider","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeLinkComponentTokensProvider"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider/linkMonoColorContentDisabled":{"type":"topic","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"linkMonoColorContentDisabled","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens","text":"MultipleColorSemanticTokens","kind":"typeIdentifier"}],"role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"Has for value token MultipleColorSemanticTokens(light: colors.colorRepositoryOpacityBlackMedium, dark: colors.colorRepositoryOpacityWhiteMedium)"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider\/linkMonoColorContentDisabled","title":"linkMonoColorContentDisabled","url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider\/linkmonocolorcontentdisabled"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"kind":"symbol","role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example.","type":"text"}],"type":"topic","title":"OUDSThemesOrange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","url":"\/documentation\/oudsthemesorange"}}}