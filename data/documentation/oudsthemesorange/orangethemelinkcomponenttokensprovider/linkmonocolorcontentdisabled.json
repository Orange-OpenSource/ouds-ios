ections":[],"references":{"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"images":[{"type":"icon","identifier":"ic_theme_orange.png"}],"kind":"symbol","type":"topic","title":"Theme Orange","url":"\/documentation\/oudsthemesorange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"},"ic_theme_orange.png":{"identifier":"ic_theme_orange.png","alt":null,"variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png","traits":["1x","light"]}],"type":"image"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"A class which wraps all "},{"inlineContent":[{"text":"component  tokens of links","type":"text"}],"type":"strong"},{"type":"text","text":" for "},{"inlineContent":[{"type":"text","text":"link"}],"type":"emphasis"},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSLink"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllLinkComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"