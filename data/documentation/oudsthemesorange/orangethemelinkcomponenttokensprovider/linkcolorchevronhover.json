rough any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Link"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"kind":"symbol","title":"OrangeThemeLinkComponentTokensProvider","navigatorTitle":[{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}]},"ic_theme_orange.svg":{"type":"image","variants":[{"traits":["1x","light"],"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.svg"}],"identifier":"ic_theme_orange.svg","alt":null},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"url":"\/documentation\/oudsthemesorange","role":"collection","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","kind":"symbol","title":"OUDSThemesOrange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","images":[{"identifier":"ic_theme_orange.svg","type":"card"},{"type":"icon","identifier":"ic_theme_orange.svg"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider/linkColorChevronHover":{"type":"topic","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"linkColorChevronHover","kind":"identifier"},{"text":": ","kind":"text"},{"text":"MultipleColorSemanticTokens","kind":"typeIdentifier","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColo