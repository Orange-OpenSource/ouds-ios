text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":") and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider","role":"symbol","kind":"symbol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeLinkComponentTokensProvider"}],"navigatorTitle":[{"kind":"identifier","text":"OrangeThemeLinkComponentTokensProvider"}]}}}