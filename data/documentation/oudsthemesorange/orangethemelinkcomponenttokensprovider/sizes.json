pe":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Link"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}],"type":"topic","title":"OrangeThemeLinkComponentTokensProvider","navigatorTitle":[{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider"},"doc://OUDS/documentation/OUDS/AllSizeSemanticTokensProvider":{"url":"\/documentation\/ouds\/allsizesemantictokensprovider","kind":"symbol","type":"topic","role":"symbol","identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllSizeSemanticTokensProvider","abstract":[{"text":"Something which provides all semantic tokens of size","type":"text"}],"title":"AllSizeSemanticTokensProvider","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AllSizeSemanticTokensProvider","kind":"identifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:4OUDS14TokensProviderP","text":"TokensProvider","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic012SizeMultipleB6TokensP","text":"SizeMultipleSemanticTokens","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic04Si