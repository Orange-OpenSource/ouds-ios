":{"abstract":[{"type":"text","text":"Has for value token MultipleColorSemanticTokens(light: colors.colorRepositoryOpacityBlackHigh, dark: colors.colorRepositoryOpacityWhiteHigher)"}],"type":"topic","kind":"symbol","role":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider\/linkMonoColorContentPressed","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"linkMonoColorContentPressed"},{"kind":"text","text":": "},{"preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens","kind":"typeIdentifier","text":"MultipleColorSemanticTokens"}],"title":"linkMonoColorContentPressed","url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider\/linkmonocolorcontentpressed"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}],"abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of links"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"link"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSLink"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllLinkComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"LinkComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"links"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Link","type":"text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"title":"OrangeThemeLinkComponentTokensProvider","kind":"symbol","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeLinkComponentTokensProvider"}],"type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider"}}}