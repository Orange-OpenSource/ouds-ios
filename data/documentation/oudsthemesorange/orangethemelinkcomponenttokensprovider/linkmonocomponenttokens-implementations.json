gh any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"inlineContent":[{"text":"Link","type":"text"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"title":"OrangeThemeLinkComponentTokensProvider","type":"topic","navigatorTitle":[{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}],"url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider","fragments":[{"kind":"keyword","text":"class"},{"text":" ","kind":"text"},{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider/linkMonoColorContentHover":{"abstract":[{"type":"text","text":"Has for value token MultipleColorSemanticTokens(light: colors.colorRepositoryNeutralEmphasizedBlack, dark: colors.colorRepositoryNeutralMutedWhite)"}],"title":"linkMonoColorContentHover","url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider\/linkmonocolorcontenthover","role":"symbol","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"linkMonoColorContentHover","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens","text":"MultipleColorSemanticTokens","kind":"typeIdentifier"}],"kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider\/linkMonoColorContentHover","type":"topic"}}}