xt","text":" "},{"inlineContent":[{"type":"text","text":"Link"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":") and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeLinkComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider","kind":"symbol","role":"symbol","title":"OrangeThemeLinkComponentTokensProvider","type":"topic"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"abstract":[{"text":"The Orange theme overrides some tokens from the basic ","type":"text"},{"type":"codeVoice","code":"OUDSTheme"},{"text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example.","type":"text"}],"kind":"symbol","url":"\/documentation\/oudsthemesorange","role":"collection","type":"topic","images":[{"identifier":"ic_theme_orange.svg","type":"icon"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","title":"Theme Orange"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider/linkSpaceColumnGapIconSmall":{"title":"linkSpaceColumnGapIconSmall","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"linkSpaceColumnGapIconSmall","kind":"identifier"},{"text":": ","kind":"text"},{"text":"SpaceSemanticToken","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena"}],"abstract":[{"text":"Has for value token spaces.spaceColumnGapXs","type":"text"}],"role":"symbol","iden