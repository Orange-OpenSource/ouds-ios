tTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"text":"links","type":"text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Link","type":"text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"kind":"symbol","role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}],"navigatorTitle":[{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider/linkSpacePaddingBlock":{"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"linkSpacePaddingBlock","kind":"identifier"},{"text":": ","kind":"text"},{"text":"SpaceSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB5Tokena","kind":"typeIdentifier"}],"abstract":[{"text":"Has for value token spaces.spacePaddingBlockMd","type":"text"}],"type":"topic","title":"linkSpacePaddingBlock","role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemelinkcomponenttokensprovider\/linkspacepaddingblock","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeLinkComponentTokensProvider\/linkSpacePaddingBlock"}}}