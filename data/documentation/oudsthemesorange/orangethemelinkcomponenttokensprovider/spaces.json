xt":" ","type":"text"},{"inlineContent":[{"type":"text","text":"Link"}],"type":"emphasis"},{"text":" components tokens are defined with raw and semantic tokens of sizes (from ","type":"text"},{"code":"AllSizeSemanticTokensProvider","type":"codeVoice"},{"text":"),","type":"text"},{"text":" ","type":"text"},{"text":"colors (from ","type":"text"},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"text":") and spaces (from ","type":"text"},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"text":").","type":"text"}],"kind":"symbol"},"doc://OUDS/documentation/OUDS/AllSpaceSemanticTokensProvider":{"url":"\/documentation\/ouds\/allspacesemantictokensprovider","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AllSpaceSemanticTokensProvider","kind":"identifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:4OUDS14TokensProviderP","text":"TokensProvider","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic013SpaceMultipleB6TokensP","text":"SpaceMultipleSemanticTokens","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"SpaceSemanticTokens","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB6TokensP"}],"kind":"symbol","type":"topic","role":"symbol","title":"AllSpaceSemanticTokensProvider","identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllSpaceSemanticTokensProvider","abstract":[{"type":"text","text":"Something which provides all semantic tokens of space"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"title":"OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","kind":"symbol","role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/doc