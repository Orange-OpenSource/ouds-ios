UDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Link"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"kind":"symbol","title":"OrangeThemeLinkComponentTokensProvider","navigatorTitle":[{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"url":"\/documentation\/oudsthemesorange","role":"collection","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","kind":"symbol","title":"OUDSThemesOrange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","images":[{"identifier":"ic_theme_orange.svg","type":"card"},{"type":"icon","identifier":"ic_theme_orange.svg"}]},"doc://OUDS/documentation/OUDS/AllSpaceSemanticTokensProvider":{"fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AllSpaceSemanticTokensProvider","kind":"identifier"},{"text":" : ","kind":"text"},{"text":"TokensProvider","preciseIdentifier":"s:4OUDS14TokensProviderP","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"SpaceMultipleSemanticTokens","preciseIdentifier":"s:18OUDSTokensSemantic013SpaceMultipleB6TokensP","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"SpaceSemanticTokens","preciseIdentifier":"s:18OUDSTokensSemantic05SpaceB6TokensP","kind":"typeIdentifier"}],