ext","text":"links"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Link"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"AllSizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":") and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"kind":"symbol","type":"topic","title":"OrangeThemeLinkComponentTokensProvider","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeLinkComponentTokensProvider","kind":"identifier"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"type":"topic","images":[{"identifier":"ic_theme_orange.svg","type":"card"},{"type":"icon","identifier":"ic_theme_orange.svg"}],"title":"OUDSThemesOrange","role":"collection","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"url":"\/documentation\/oudsthemesorange","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"},"ic_theme_orange.svg":{"variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.svg","traits":["1x","light"]}],"alt":null,"identifier":"ic_theme_orange.svg","type":"image"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeLinkComponentTokensProvider/linkMonoColorContentHover":{"type":"topic","title":"linkMonoColorContentHover","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"linkMonoColorContentHover","kind":"iden