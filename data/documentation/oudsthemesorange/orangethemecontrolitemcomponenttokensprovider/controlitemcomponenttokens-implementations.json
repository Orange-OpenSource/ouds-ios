cumentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider/controlItemSizeIcon":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/controlItemSizeIcon","title":"controlItemSizeIcon","role":"symbol","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"controlItemSizeIcon"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena","text":"SizeSemanticToken"}],"kind":"symbol","abstract":[{"type":"text","text":"Has for value token sizes.sizeIconWithLabelLargeSizeMd"}],"type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider\/controlitemsizeicon"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider":{"role":"symbol","title":"OrangeThemeControlItemComponentTokensProvider","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"type":"topic","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"abstract":[{"type":"text","text":"A class which wraps all "},{"inlineContent":[{"type":"text","text":"component  tokens"}],"type":"strong"},{"type":"text","text":" for components with a "},{"inlineContent":[{"type":"text","text":"control item"}],"type":"emphasis"},{"type":"text","text":" layout like "},{"code":"OUDSSwitch","type":"codeVoice"},{"type":"text","text":", "},{"code":"OUDSRadioButton","type":"codeVoice"},{"type":"text","text":" and "},{"code":"OUDSCheckboItem","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"code":"AllControlItemComponentTokensProvider","type":"codeVoice"},{"type":"text","text":" implementation inside "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"ControlItemComponentTokens"},{"type":"text","text":" so as to expose the component tokens for control-item-layout-basded components through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Such components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"SizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"),  colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")  and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"role":"collection","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"url":"\/documentation\/oudsthemesorange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","title":"OUDSThemesOrange"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider/controlItemBorderRadiusItemOnly":{"role":"symbol","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"controlItemBorderRadiusItemOnly","kind":"identifier"},{"text":": ","kind":"text"},{"text":"BorderRadiusSemanticToken","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic012BorderRadiusB5Tokena"}],"type":"topic","kind":"symbol","title":"controlItemBorderRadiusItemOnly","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/controlItemBorderRadiusItemOnly","abstract":[{"text":"Has for value token borders.borderRadiusDefault","type":"text"}],"url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider\/controlitemborderradiusitemonly"}}}