der"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"navigatorTitle":[{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider","type":"topic","title":"OrangeThemeControlItemComponentTokensProvider","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"type":"topic","url":"\/documentation\/oudsthemesorange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"images":[{"identifier":"ic_theme_orange.png","type":"icon"}],"role":"collection","title":"OUDSThemesOrange","kind":"symbol"}}}