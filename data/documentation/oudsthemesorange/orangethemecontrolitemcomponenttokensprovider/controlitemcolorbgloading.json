e/OrangeThemeControlItemComponentTokensProvider":{"kind":"symbol","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"text":"component  tokens","type":"text"}]},{"type":"text","text":" for components with a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"control item"}]},{"type":"text","text":" layout like "},{"type":"codeVoice","code":"OUDSSwitch"},{"type":"text","text":", "},{"type":"codeVoice","code":"OUDSRadioButton"},{"type":"text","text":" and "},{"type":"codeVoice","code":"OUDSCheckboItem"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllControlItemComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"ControlItemComponentTokens"},{"type":"text","text":" so as to expose the component tokens for control-item-layout-basded components through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Such components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"SizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"),  colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")  and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"navigatorTitle":[{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider","type":"topic","title":"OrangeThemeControlItemComponentTokensProvider","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider/controlItemColorBgLoading":{"type":"topic","role":"symbol","title":"controlItemColorBgLoading","kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider\/controlitemcolorbgloading","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"controlItemColorBgLoading","kind":"identifier"},{"text":": ","kind":"text"},{"text":"MultipleColorSemanticTokens","kind":"typeIdentifier","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/controlItemColorBgLoading","abstract":[{"type":"text","text":"Has for value token colors.colorActionSupportLoading"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"type":"topic","url":"\/do