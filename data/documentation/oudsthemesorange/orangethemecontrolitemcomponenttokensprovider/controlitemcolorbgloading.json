om "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"),  colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")  and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"text":"class","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"kind":"symbol","type":"topic"},"ic_theme_orange.png":{"variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png","traits":["1x","light"]}],"type":"image","alt":null,"identifier":"ic_theme_orange.png"}}}