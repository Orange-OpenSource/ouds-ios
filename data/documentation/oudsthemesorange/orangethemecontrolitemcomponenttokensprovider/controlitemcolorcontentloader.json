UDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Such components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"SizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"),  colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")  and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"title":"OrangeThemeControlItemComponentTokensProvider","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider/controlItemColorContentLoader":{"fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"controlItemColorContentLoader"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"MultipleColorSemanticTokens","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens"}],"abstract":[{"text":"Has for value token colors.colorContentDefault","type":"text"}],"url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider\/controlitemcolorcontentloader","kind":"symbol","type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/controlItemColorContentLoader","role":"symbol","title":"controlItemColorContentLoader"},"doc://OUDSThemesOrange/documentation/OUDSThemesOra