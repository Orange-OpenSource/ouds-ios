item-layout-basded components through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Such components tokens are defined with raw and semantic tokens of sizes (from "},{"code":"SizeSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"code":"AllBorderSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),  colors (from "},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")  and spaces (from "},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","type":"topic","kind":"symbol","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider/controlItemBorderRadius":{"kind":"symbol","role":"symbol","type":"topic","abstract":[{"text":"Has for value token borders.borderRadiusNone","type":"text"}],"url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider\/controlitemborderradius","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"controlItemBorderRadius","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:18OUDSTokensSemantic012BorderRadiusB5Tokena","text":"BorderRadiusSemanticToken","kind":"typeIdentifier"}],"title":"controlItemBorderRadius","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/controlItemBorderRadius"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"title":"OUDSThemesOrange","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","type":"topic","abstract":[{"text":"The Orang