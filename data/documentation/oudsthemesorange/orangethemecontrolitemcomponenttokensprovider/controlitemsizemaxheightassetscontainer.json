emantic tokens of sizes (from "},{"type":"codeVoice","code":"SizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"),  colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")  and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"navigatorTitle":[{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider","type":"topic","title":"OrangeThemeControlItemComponentTokensProvider","role":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider/controlItemSizeMaxHeightAssetsContainer":{"title":"controlItemSizeMaxHeightAssetsContainer","abstract":[{"type":"text","text":"Has for value token DimensionRawTokens.dimension1200"}],"kind":"symbol","type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/controlItemSizeMaxHeightAssetsContainer","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"controlItemSizeMaxHeightAssetsContainer","kind":"identifier"},{"text":": ","kind":"text"},{"text":"SizeSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic04SizeB5Tokena","kind":"typeIdentifier"}],"role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider\/controlitemsizemaxheightassetscontainer"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"typ