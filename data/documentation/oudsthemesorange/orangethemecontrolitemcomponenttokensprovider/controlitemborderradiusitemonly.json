text","text":"),  colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")  and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"type":"topic","role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider","title":"OrangeThemeControlItemComponentTokensProvider","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeControlItemComponentTokensProvider","kind":"identifier"}],"kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider"}}}