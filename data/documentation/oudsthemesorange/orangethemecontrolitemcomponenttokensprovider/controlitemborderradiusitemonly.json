ensProvider/controlItemBorderRadiusItemOnly":{"kind":"symbol","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"controlItemBorderRadiusItemOnly","kind":"identifier"},{"text":": ","kind":"text"},{"text":"BorderRadiusSemanticToken","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic012BorderRadiusB5Tokena"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/controlItemBorderRadiusItemOnly","abstract":[{"text":"Has for value token borders.borderRadiusDefault","type":"text"}],"title":"controlItemBorderRadiusItemOnly","type":"topic","role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider\/controlitemborderradiusitemonly"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider":{"kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"type":"topic","fragments":[{"text":"class","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeControlItemComponentTokensProvider"}],"url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens"}]},{"type":"text","text":" for components with a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"control item"}]},{"type":"text","text":" layout like "},{"type":"codeVoice","code":"OUDSSwitch"},{"type":"text","text":", "},{"type":"codeVoice","code":"OUDSRadioButton"},{"type":"text","text":" and "},{"type":"codeVoice","code":"OUDSCheckboItem"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllControlItemComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","type":"reference","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"ControlItemComponentTokens"},{"type":"text","text":" so as to expose the component tokens for control-item-layout-basded components through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Such components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"SizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"),  colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")  and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"role":"symbol","title":"OrangeThemeControlItemComponentTokensProvider","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","abstract":[{"text":"The Orange theme overrides some tokens from the basic ","type":"text"},{"code":"OUDSTheme","type":"codeVoice"},{"text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example.","type":"text"}],"role":"collection","title":"OUDSThemesOrange","kind":"symbol","type":"topic","url":"\/documentation\/oudsthemesorange"}}}