documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/colors","kind":"symbol","role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider\/colors","title":"colors","fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"colors"},{"kind":"text","text":": any "},{"kind":"typeIdentifier","preciseIdentifier":"s:4OUDS30AllColorSemanticTokensProviderP","text":"AllColorSemanticTokensProvider"}]},"doc://OUDS/documentation/OUDS/AllColorSemanticTokensProvider":{"title":"AllColorSemanticTokensProvider","role":"symbol","kind":"symbol","type":"topic","identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllColorSemanticTokensProvider","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"AllColorSemanticTokensProvider"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","preciseIdentifier":"s:4OUDS14TokensProviderP","text":"TokensProvider"},{"kind":"text","text":", "},{"kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic013ColorMultipleB6TokensP","text":"ColorMultipleSemanticTokens"},{"kind":"text","text":", "},{"kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic05ColorB6TokensP","text":"ColorSemanticTokens"}],"url":"\/documentation\/ouds\/allcolorsemantictokensprovider","abstract":[{"text":"Something which provides all semantic tokens of colors","type":"text"}]},"ic_theme_orange.png":{"identifier":"ic_theme_orange.png","variants":[{"traits":["1x","light"],"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png"}],"alt":null,"type":"image"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider":{"title":"OrangeThemeControlItemComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider","role":"symbol","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeControlItemComponentTokensProvider","kind":"identifier"}],"type":"topic","abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens"}]},{"type":"text","text":" for components with a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"control item"}]},{"type":"text","text":" layout like "},{"type":"codeVoice","code":"OUDSSwitch"},{"type":"text","text":", "},{"type":"codeVoice","code":"OUDSRadioButton"},{"type":"text","text":" and "},{"type":"codeVoice","code":"OUDSCheckboItem"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllControlItemComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider","type":"reference","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"ControlItemComponentTokens"},{"type":"text","text":" so as to expose the component tokens for control-item-layout-basded components through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Such components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"SizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"),  colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")  and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"navigatorTitle":[{"text":"OrangeThemeControlItemComponentTokensProvider","kind":"identifier"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"title":"Theme Orange","kind":"symbol","type":"topic","url":"\/documentation\/oudsthemesorange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"role":"collection","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","images":[{"identifier":"ic_theme_orange.png","type":"icon"}]}}}