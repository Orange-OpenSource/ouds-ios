ext"},{"text":"AllColorSemanticTokensProvider","kind":"identifier"},{"text":" : ","kind":"text"},{"text":"TokensProvider","preciseIdentifier":"s:4OUDS14TokensProviderP","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"ColorMultipleSemanticTokens","preciseIdentifier":"s:18OUDSTokensSemantic013ColorMultipleB6TokensP","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"ColorSemanticTokens","preciseIdentifier":"s:18OUDSTokensSemantic05ColorB6TokensP","kind":"typeIdentifier"}],"type":"topic","abstract":[{"text":"Something which provides all semantic tokens of colors","type":"text"}],"kind":"symbol","role":"symbol","title":"AllColorSemanticTokensProvider"},"ic_theme_orange.svg":{"type":"image","variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.svg","traits":["1x","light"]}],"identifier":"ic_theme_orange.svg","alt":null},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider/colors":{"url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider\/colors","type":"topic","role":"symbol","fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"colors","kind":"identifier"},{"text":": any ","kind":"text"},{"text":"AllColorSemanticTokensProvider","kind":"typeIdentifier","preciseIdentifier":"s:4OUDS30AllColorSemanticTokensProviderP"}],"kind":"symbol","abstract":[{"text":"Provider of color semantic tokens to use for control-item-layout-based  colors","type":"text"}],"title":"colors","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider\/colors"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeControlItemComponentTokensProvider":{"navigatorTitle":[{"text":"OrangeThemeControlItemComponentTokensProvider","kind":"identifier"}],"abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens"}]},{"type":"text","text":" for components with a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"control item"}]},{"type":"text","text":" layout like "},{"type":"codeVoice","code":"OUDSSwitch"},{"type":"text","text":", "},{"type":"codeVoice","code":"OUDSRadioButton"},{"type":"text","text":" and "},{"type":"codeVoice","code":"OUDSCheckboItem"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllControlItemComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"ControlItemComponentTokens"},{"type":"text","text":" so as to expose the component tokens for control-item-layout-basded components through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Such components tokens are defined with raw and semantic tokens of sizes (from "},{"type":"codeVoice","code":"SizeSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"),  colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":")  and spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")."}],"type":"topic","role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider","title":"OrangeThemeControlItemComponentTokensProvider","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeControlItemComponentTokensProvider","kind":"identifier"}],"kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"url":"\/documentation\/oudsthemesorange","role":"collection","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","kind":"symbol","title":"OUDSThemesOrange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","images":[{"identifier":"ic_theme_orange.svg","type":"card"},{"type":"icon","identifier":"ic_theme_orange.svg"}]}}}