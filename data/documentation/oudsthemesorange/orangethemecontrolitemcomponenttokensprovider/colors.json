borders (from ","type":"text"},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"text":"),  colors (from ","type":"text"},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"text":")  and spaces (from ","type":"text"},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"text":").","type":"text"}],"url":"\/documentation\/oudsthemesorange\/orangethemecontrolitemcomponenttokensprovider","kind":"symbol","type":"topic","navigatorTitle":[{"text":"OrangeThemeControlItemComponentTokensProvider","kind":"identifier"}],"fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeControlItemComponentTokensProvider","kind":"identifier"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeControlItemComponentTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"kind":"symbol","role":"collection","title":"OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"},"doc://OUDS/documentation/OUDS/AllColorSemanticTokensProvider":{"abstract":[{"text":"Something which provides all semantic tokens of colors","type":"text"}],"url":"\/documentation\/ouds\/allcolorsemantictokensprovider","role":"symbol","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AllColorSemanticTokensProvider","kind":"identifier"},{"text":" : ","kind":"text"},{"text":"TokensProvider","kind":"typeIdentifier","preciseIdentifier":"s:4OUDS14TokensProviderP"},{"text":", ","kind":"text"},{"text":"ColorMultipleSemanticTokens","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic013ColorMultipleB6TokensP"},{"te