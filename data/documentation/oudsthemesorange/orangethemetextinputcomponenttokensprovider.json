oice"},{"text":" so as to expose the component tokens for ","type":"text"},{"inlineContent":[{"text":"input text","type":"text"}],"type":"emphasis"},{"text":" through any ","type":"text"},{"code":"OUDSTheme","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"inlineContent":[{"text":"Inout text","type":"text"}],"type":"emphasis"},{"text":" components tokens are defined with raw and semantic tokens of colors (","type":"text"},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"text":"),","type":"text"},{"text":" ","type":"text"},{"text":"borders (from ","type":"text"},{"code":"AllBorderSemanticTokensProvider","type":"codeVoice"},{"text":"), spaces (from ","type":"text"},{"code":"AllSpaceSemanticTokensProvider","type":"codeVoice"},{"text":")","type":"text"},{"text":" ","type":"text"},{"text":"and  dimensions (from ","type":"text"},{"code":"AllDimensionSemanticTokensProvider","type":"codeVoice"},{"text":").","type":"text"}],"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeTextInputComponentTokensProvider","kind":"identifier"}]}]},{"kind":"content","content":[{"level":2,"anchor":"overview","text":"Overview","type":"heading"},{"syntax":"swift","code":["    \/\/ Define your own provider for input text component tokens","    \/\/ by inheriting from existing provider","    class CustomTextInputComponentTokensProvider: OrangeThemeTextInputComponentTokensProvider {","","        \/\/ Then override the text input component tokens you want.","","        override var textInputSizeMaxWidth: SizeSemanticToken { DimensionRawTokens.dimension3000 }","","        override var textInputSpacePaddingBlockDefault: SpaceSemanticToken { spaces.spacePaddingInlineSm }","","        override var textInputColorBorderEnabled: MultipleColorSemanticTokens { colors.colorRepositoryOpacityBlackHigher }",