hips","title":"Conforms To","identifiers":["doc:\/\/OUDS\/documentation\/OUDS\/AllTextInputComponentTokensProvider","doc:\/\/OUDSThemesOrange\/19OUDSTokensComponent09TextInputB6TokensP","doc:\/\/OUDSThemesOrange\/s8CopyableP","doc:\/\/OUDS\/documentation\/OUDS\/TokensProvider"]}],"hierarchy":{"paths":[["doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["swift"],"tokens":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTextInputComponentTokensProvider"}],"platforms":["macOS"]}]},{"content":[{"anchor":"overview","type":"heading","level":2,"text":"Overview"},{"type":"codeListing","syntax":"swift","code":["    \/\/ Define your own provider for input text component tokens","    \/\/ by inheriting from existing provider","    class CustomTextInputComponentTokensProvider: OrangeThemeTextInputComponentTokensProvider {","","        \/\/ Then override the text input component tokens you want.","","        override var textInputSizeMaxWidth: SizeSemanticToken { DimensionRawTokens.dimension3000 }","","        override var textInputSpacePaddingBlockDefault: SpaceSemanticToken { spaces.spacePaddingInlineSm }","","        override var textInputColorBorderEnabled: MultipleColorSemanticTokens { colors.colorRepositoryOpacityBlackHigher }","","        override var textInputBorderWidthDefault: BorderWidthSemanticToken { borders.borderWidthMedium}","","        \/\/ ...","    }","","    \/\/ Or define your own provider from scratch","    class CustomTextInputComponentTokensProvider: TextInputComponentTokens {","","        \/\/ And implement hunders of tokens.","        \/\/ You are allowed to give semantic tokens providers if you want to define values.","    }"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Then, you can give this "},{"type":"codeVoice","code":"CustomTextInputComponentTokensProvider"},{"type":"text","text":" to your own theme implementation:"}]},{"type":"codeListing","syntax":"swift","code":["class LocalTheme: OrangeTheme {","","    override init() {","        super.init(textInput: CustomTextInputComponentTokensProvider())","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"or to an already existing theme for example:"}]},{"type":"codeListing","syntax":"swift","code":["    OrangeTheme(textInput: CustomTextInputComponentTokensProvider())"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"It is also possible to use your own semantic tokens providers for this component tokens providers:"}]},{"type":"codeListing","syntax":"swift","code":["    \/\/ Uses by default here:","    \/\/ - OrangeThemeSizeSemanticTokensProvider for sizes","    \/\/ - OrangeThemeColorSemanticTokensProvider for colors","    \/\/ - OrangeThemeBorderSemanticTokensProvider for borders","    \/\/ - OrangeThemeSpaceSemanticTokensProvider for spaces","    \/\/ - OrangeThemeDimensionSemanticTokensProvider for dimensions","    let inputTextComponentTokensProvider = OrangeThemeTextInputComponentTokensProvider()","","    \/\/ Or use your own size, border, color, space and dimension semantic tokens providers (or only some)","    let inputTextComponentTokensProvider = OrangeThemeTextInputComponentTokensProvider(","                                                sizes: CustomSizeSemanticTokensProvider(),","                                                borders: CustomBorderSemanticTokensProvider(),","                                                colors: CustomColorSemanticTokensProvider(),","                                                space: CustomSpaceSemanticTokensProvider(),","                                                dimensions: CustomDimensionSemanticTokensProvider())"]},{"content":[{"inlineContent":[{"type":"text","text":"0.17.0"}],"type":"paragraph"}],"type":"aside","style":"note","name":"Since"}],"kind":"content"}],"abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of input text"}]},{"type":"text","text":" for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"input text"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSTextInput"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllTextInputComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"TextInputComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"input text"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Inout text","type":"text"}]},{"type":"text","text":" components tokens are defined with raw and semantic tokens of colors ("},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":"), spaces (from "},{"type":"codeVoice","code":"AllSpaceSemanticTokensProvider"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"and  dimensions (from "},{"type":"codeVoice","code":"AllDimensionSemanticTokensProvider"},{"type":"text","text":")."}],"schemaVersion":{"minor":3,"major":0,"patch":0},"kind":"symbol","identifier":{"url":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","interfaceLanguage":"swift"},"sections":[],"references":{"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"images":[{"type":"icon","identifier":"ic_theme_orange.svg"}],"type":"topic","role":"collection","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","url":"\/documentation\/oudsthemesorange","title":"Theme Orange","abstract":[{"text":"The Orange theme overrides some tokens from the basic ","type":"text"},{"type":"codeVoice","code":"OUDSTheme"},{"text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example.","type":"text"}]},"doc://OUDS/documentation/OUDS/TokensProvider":{"identifier":"doc:\/\/OUDS\/documentation\/OUDS\/TokensProvider","title":"TokensProvider","kind":"symbol","type":"topic","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"TokensProvider"}],"abstract":[{"type":"text","text":"Protocol to add to any semantic or component tokens provider so as to be gather with all providers and given to the theme for initialization"}],"role":"symbol","url":"\/documentation\/ouds\/tokensprovider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTextInputComponentTokensProvider":{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","fragments