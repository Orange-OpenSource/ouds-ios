{"relationshipsSections":[{"title":"Conforms To","identifiers":["doc:\/\/OUDS\/documentation\/OUDS\/AllTextInputComponentTokensProvider","doc:\/\/OUDSThemesOrange\/19OUDSTokensComponent09TextInputB6TokensP","doc:\/\/OUDSThemesOrange\/s8CopyableP","doc:\/\/OUDS\/documentation\/OUDS\/TokensProvider"],"type":"conformsTo","kind":"relationships"}],"metadata":{"externalID":"s:16OUDSThemesOrange0B37ThemeTextInputComponentTokensProviderC","title":"OrangeThemeTextInputComponentTokensProvider","modules":[{"name":"OUDSThemesOrange"}],"navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTextInputComponentTokensProvider"}],"roleHeading":"Class","symbolKind":"class","role":"symbol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTextInputComponentTokensProvider"}]},"primaryContentSections":[{"declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeTextInputComponentTokensProvider","kind":"identifier"}]}],"kind":"declarations"},{"content":[{"text":"Overview","level":2,"anchor":"overview","type":"heading"},{"syntax":"swift","type":"codeListing","code":["    \/\/ Define your own provider for input text component tokens","    \/\/ by inheriting from existing provider","    class CustomTextInputComponentTokensProvider: OrangeThemeTextInputComponentTokensProvider {","","        \/\/ Then override the text input component tokens you want.","","        override var textInputSizeMaxWidth: SizeSemanticToken { DimensionRawTokens.dimension3000 }","","        override var textInputSpacePaddingBlockDefault: SpaceSemanticToken { spaces.spacePaddingInlineSm }","","        override var textInputColorBorderEnabled: MultipleColorSemanticTokens { colors.colorRepositoryOpacityBlackHigher }","","        override var textInputBorderWidthDefault: BorderWidthSemanticToken { borders.borderWidthMedium}","","        \/\/ ...","    }","","    \/\/ Or define your own provider from scratch","    class CustomTextInputComponentTokensProvider: TextInputComponentTokens {","","        \/\/ And implement hunders of tokens.","        \/\/ You are allowed to give semantic tokens providers if you want to define values.","    }"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Then, you can give this "},{"type":"codeVoice","code":"CustomTextInputComponentTokensProvider"},{"type":"text","text":" to your own theme implementation:"}]},{"syntax":"swift","type":"codeListing","code":["class LocalTheme: OrangeTheme {","","    override init() {","        super.init(textInput: CustomTextInputComponentTokensProvider())","    }","}"]},{"type":"paragraph","inlineContent":[{"text":"or to an already existing theme for example:","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["    OrangeTheme(textInput: CustomTextInputComponentTokensProvider())"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"It is also possible to use your own semantic tokens providers for this component tokens providers:"}]},{"syntax":"swift","type":"codeListing","code":["    \/\/ Uses by default here:","    \/\/ - OrangeThemeSizeSemanticTokensProvider for sizes","    \/\/ - OrangeThemeColorSemanticTokensProvider for colors","    \/\/ - OrangeThemeBorderSemanticTokensProvider for borders","    \/\/ - OrangeThemeSpaceSemanticTokensProvider for spaces","    \/\/ - OrangeThemeDimensionSemanticTokensProvider for dimensions","    let inputTextComponentTokensProvider = OrangeThemeTextInputComponentTokensProvider()","","    \/\/ Or use your own size, border, color, space and dimension semantic tokens providers (or only some)","    let inputTextComponentTokensProvider = OrangeThemeTextInputComponentTokensProvider(","                                                sizes: CustomSizeSemanticTokensProvider(),","                                                borders: CustomBorderSemanticTokensProvider(),","                                                colors: CustomColorSemanticTokensProvider(),","                                                space: CustomSpaceSemanticTokensProvider(),","                                                dimensions: CustomDimensionSemanticTokensProvider())"]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"0.17.0"}]}],"name":"Since","type":"aside","style":"note"}],"kind":"content"}],"schemaVersion":{"minor":3,"major":0,"patch":0},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/oudsthemesorange\/orangethemetextinputcomponenttokensprovider"]}],"abstract":[{"type":"text","text":"A class which wraps all "},{"inlineContent":[{"type":"text","text":"component  tokens of input text"}],"type":"strong"},{"type":"text","text":" for "},{"inlineContent":[{"type":"text","text":"input text"}],"type":"emphasis"},{"type":"text","text":" objects like "},{"code":"OUDSTextInput","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"code":"AllTextInputComponentTokensProvider","type":"codeVoice"},{"type":"text","text":" implementation inside "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTextInputComponentTokensProvider","type":"reference","isActive":true},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":"