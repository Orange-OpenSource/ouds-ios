g inputs"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"role":"symbol","title":"OrangeThemeTagInputComponentTokensProvider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTagInputComponentTokensProvider"}],"kind":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemetaginputcomponenttokensprovider"},"doc://OUDS/documentation/OUDS/AllBorderSemanticTokensProvider":{"url":"\/documentation\/ouds\/allbordersemantictokensprovider","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AllBorderSemanticTokensProvider","kind":"identifier"},{"text":" : ","kind":"text"},{"text":"TokensProvider","kind":"typeIdentifier","preciseIdentifier":"s:4OUDS14TokensProviderP"},{"text":", ","kind":"text"},{"text":"BorderSemanticTokens","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic06BorderB6TokensP"}],"role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"Something which provides all semantic tokens of border"}],"identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllBorderSemanticTokensProvider","type":"topic","title":"AllBorderSemanticTokensProvider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"url":"\/documentation\/oudsthemesorange","role":"collection","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","kind":"symbol","title":"OUDSThemesOrange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","images":[{"identifier":"ic_theme_orange.svg","type":"card"