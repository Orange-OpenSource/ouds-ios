ent tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"tag inputs"}]},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Tag inputs"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and borders (from "},{"code":"AllBorderSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"title":"OrangeThemeTagInputComponentTokensProvider","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagInputComponentTokensProvider","type":"topic","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTagInputComponentTokensProvider"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTagInputComponentTokensProvider"}],"url":"\/documentation\/oudsthemesorange\/orangethemetaginputcomponenttokensprovider"},"doc://OUDS/documentation/OUDS/AllBorderSemanticTokensProvider":{"fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AllBorderSemanticTokensProvider","kind":"identifier"},{"text":" : ","kind":"text"},{"text":"TokensProvider","kind":"typeIdentifier","preciseIdentifier":"s:4OUDS14TokensProviderP"},{"text":", ","kind":"text"},{"text":"BorderSemanticTokens","kind":"typeIdentifier","preciseIdentifier":"s:18OUDSTokensSemantic06BorderB6TokensP"}],"title":"AllBorderSemanticTokensProvider","kind":"symbol","identifier":"doc:\/\/OUDS\/documentation\/OUDS\/AllBorderSemanticTokensProvider","type":"topic","abstract":[{"text":"Something which provides all semantic tokens of border","type":"text"}],"url":"\/documentation\/ouds\/allbordersemantictokensprovider","role":"symbol"},"doc