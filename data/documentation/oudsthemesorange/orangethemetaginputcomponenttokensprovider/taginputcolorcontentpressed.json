rotocol "},{"code":"TagInputComponentTokens","type":"codeVoice"},{"type":"text","text":" so as to expose the component tokens for "},{"inlineContent":[{"text":"tag inputs","type":"text"}],"type":"emphasis"},{"type":"text","text":" through any "},{"code":"OUDSTheme","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"inlineContent":[{"type":"text","text":"Tag inputs"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"code":"AllColorSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and borders (from "},{"code":"AllBorderSemanticTokensProvider","type":"codeVoice"},{"type":"text","text":")."}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTagInputComponentTokensProvider"}],"kind":"symbol","type":"topic","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTagInputComponentTokensProvider"}],"title":"OrangeThemeTagInputComponentTokensProvider","url":"\/documentation\/oudsthemesorange\/orangethemetaginputcomponenttokensprovider"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTagInputComponentTokensProvider/tagInputColorContentPressed":{"title":"tagInputColorContentPressed","abstract":[{"type":"text","text":"Has for value token colors.colorActionPressed"}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagInputComponentTokensProvider\/tagInputColorContentPressed","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"tagInputColorContentPressed"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"c:@M@OUDSTokensSemantic@objc(cs)MultipleColorSemanticTokens","text":"MultipleColorSemanticTokens"}],"kind":"symbol","role":"symbol","url":"\/documentation\/oudsthemesorange\/orangethemetaginputcom