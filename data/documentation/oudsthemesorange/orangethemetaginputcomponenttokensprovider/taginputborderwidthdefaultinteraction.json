mentation\/oudsthemesorange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"type":"topic","title":"OUDSThemesOrange","role":"collection","kind":"symbol","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","images":[{"identifier":"ic_theme_orange.png","type":"icon"}]},"ic_theme_orange.png":{"identifier":"ic_theme_orange.png","type":"image","alt":null,"variants":[{"traits":["1x","light"],"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png"}]},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTagInputComponentTokensProvider/tagInputBorderWidthDefaultInteraction":{"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"tagInputBorderWidthDefaultInteraction","kind":"identifier"},{"text":": ","kind":"text"},{"text":"BorderWidthSemanticToken","preciseIdentifier":"s:18OUDSTokensSemantic011BorderWidthB5Tokena","kind":"typeIdentifier"}],"type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemetaginputcomponenttokensprovider\/taginputborderwidthdefaultinteraction","title":"tagInputBorderWidthDefaultInteraction","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagInputComponentTokensProvider\/tagInputBorderWidthDefaultInteraction","abstract":[{"text":"Has for value token borders.borderWidthMedium","type":"text"}],"role":"symbol","kind":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTagInputComponentTokensProvider":{"type":"topic","url":"\/documentation\/oudsthemesorange\/orangethemetaginputcomponenttokensprovider","title":"OrangeThemeTagInputComponentTokensProvider","kind":"symbol","role":"symbol","navigatorTitle":[{"text":"OrangeThemeTagInputComponentTokensProvider","kind":"identifier"}],"abstract":[{"type":"text","text":"A class which wraps all "},{"inlineContent":[{"type":"text","text":"component  tokens of tag input"}],"type":"strong"},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSTagInput"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllTagInputComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagInputComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"TagInputComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"inlineContent":[{"type":"text","text":"tag inputs"}],"type":"emphasis"},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"inlineContent":[{"type":"text","text":"Tag inputs"}],"type":"emphasis"},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagInputComponentTokensProvider","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrangeThemeTagInputComponentTokensProvider","kind":"identifier"}]}}}