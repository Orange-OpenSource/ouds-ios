sProvider":{"abstract":[{"type":"text","text":"A class which wraps all "},{"type":"strong","inlineContent":[{"type":"text","text":"component  tokens of tag input"}]},{"type":"text","text":" objects like "},{"type":"codeVoice","code":"OUDSTagInput"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Contains also references to semantic tokens providers so as to be able to use them to define the component tokens."},{"type":"text","text":" "},{"type":"text","text":"This provider should be integrated as a "},{"type":"codeVoice","code":"AllTagInputComponentTokensProvider"},{"type":"text","text":" implementation inside "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" so as to provide"},{"type":"text","text":" "},{"type":"text","text":"all tokens to the users. It helps users to override some of the tokens and assign them to an "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" implementation to use."},{"type":"text","text":" "},{"type":"text","text":"Custom themes can use subclass of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagInputComponentTokensProvider"},{"type":"text","text":" and apply the provider they need."},{"type":"text","text":" "},{"type":"text","text":"It implements also the protocol "},{"type":"codeVoice","code":"TagInputComponentTokens"},{"type":"text","text":" so as to expose the component tokens for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"tag inputs"}]},{"type":"text","text":" through any "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Tag inputs","type":"text"}]},{"type":"text","text":" components tokens are defined with semantic tokens of colors (from "},{"type":"codeVoice","code":"AllColorSemanticTokensProvider"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"and borders (from "},{"type":"codeVoice","code":"AllBorderSemanticTokensProvider"},{"type":"text","text":")."}],"type":"topic","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagInputComponentTokensProvider","title":"OrangeThemeTagInputComponentTokensProvider","navigatorTitle":[{"kind":"identifier","text":"OrangeThemeTagInputComponentTokensProvider"}],"role":"symbol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OrangeThemeTagInputComponentTokensProvider"}],"url":"\/documentation\/oudsthemesorange\/orangethemetaginputcomponenttokensprovider","kind":"symbol"},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange":{"title":"Theme Orange","kind":"symbol","type":"topic","url":"\/documentation\/oudsthemesorange","abstract":[{"type":"text","text":"The Orange theme overrides some tokens from the basic "},{"type":"codeVoice","code":"OUDSTheme"},{"type":"text","text":" and should be seen as the default theme for the OUDS library with the Orange brand colors for example."}],"role":"collection","identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange","images":[{"identifier":"ic_theme_orange.png","type":"icon"}]},"ic_theme_orange.png":{"variants":[{"url":"\/images\/OUDSThemesOrange\/ic_theme_orange.png","traits":["1x","light"]}],"type":"image","identifier":"ic_theme_orange.png","alt":null},"doc://OUDSThemesOrange/documentation/OUDSThemesOrange/OrangeThemeTagInputComponentTokensProvider/tagInputColorBgPressed":{"url":"\/documentation\/oudsthemesorange\/orangethemetaginputcomponenttokensprovider\/taginputcolorbgpressed","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Has for value token colors.colorActionSupportPressed"}],"identifier":"doc:\/\/OUDSThemesOrange\/documentation\/OUDSThemesOrange\/OrangeThemeTagInputComponentTokensProvider\/tagInputColorBgPressed","role":"symbol","title":"tagInputColorBgPressed","fragments":[{"text":"var","kind":"keyword"},{"text":" "