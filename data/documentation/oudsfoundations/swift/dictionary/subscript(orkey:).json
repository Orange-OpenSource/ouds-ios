{"hierarchy":{"paths":[["doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations","doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/Swift","doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/Swift\/Dictionary"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"subscript"},{"kind":"text","text":"("},{"kind":"externalParam","text":"orKey"},{"kind":"text","text":" "},{"kind":"internalParam","text":"key"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Key"},{"kind":"text","text":") -> "},{"preciseIdentifier":"s:SS","kind":"typeIdentifier","text":"String"},{"kind":"text","text":" { "},{"kind":"keyword","text":"get"},{"kind":"text","text":" }"}],"platforms":["macOS"],"languages":["swift"]}]},{"parameters":[{"name":"key","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The key to use to get a value"}]}]}],"kind":"parameters"},{"content":[{"type":"heading","anchor":"overview","text":"Overview","level":2},{"inlineContent":[{"type":"text","text":"In fact if it seems some rules are missing, or weight are not managed, if the font family only is returned"},{"type":"text","text":" "},{"type":"text","text":"the user has an higher probability to use the expected font. Because if a mashup of family name and weight is returned"},{"type":"text","text":" "},{"type":"text","text":"it can produce a PostScript identifier not available, so not usable, so implying a fallback to default system font like "},{"type":"emphasis","inlineContent":[{"type":"text","text":"San Francisco"}]},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Because PostScript identifiers in most of cases do not have whitespaces, remove them."}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["    \/\/ Given a dictionary with one value","    let myDict: PostScriptFontNamesMap = [PSFNMK(\"Arial\", Font.Weight.bold): \"Aria-BoldMT\"]","","    \/\/ The given value is returned if the key exists","    let keyExists = myDict[PSFNMK(\"Arial\", Font.Weight.bold)] \/\/ Arial-BoldMT","","    \/\/ But if the key does not exist, the only the key is returned","    let keyDoesNotExist = myDict[PSFNMK(\"Luciole\", Font.Weight.bold)] \/\/ Luciole"]}],"kind":"content"}],"schemaVersion":{"patch":0,"major":0,"minor":3},"metadata":{"roleHeading":"Instance Subscript","title":"subscript(orKey:)","symbolKind":"subscript","extendedModule":"Swift","conformance":{"conformancePrefix":[{"type":"text","text":"Conforms when"}],"constraints":[{"type":"codeVoice","code":"Key"},{"type":"text","text":" is "},{"type":"codeVoice","code":"PostScriptFontNamesMapKey"},{"type":"text","text":" and "},{"type":"codeVoice","code":"Value"},{"type":"text","text":" is "},{"type":"codeVoice","code":"String"},{"type":"text","text":"."}],"availabilityPrefix":[{"type":"text","text":"Available when"}]},"modules":[{"relatedModules":["Swift"],"name":"Foundations"}],"externalID":"s:SD15OUDSFoundationsAA25PostScriptFontNamesMapKeyVRszSSRs_rlE02orG0SSAC_tcip","role":"symbol","fragments":[{"kind":"keyword","text":"subscript"},{"kind":"text","text":"("},{"kind":"externalParam","text":"orKey"},{"kind":"text","text":" _: "},{"kind":"typeIdentifier","text":"Key"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"}]},"sections":[],"abstract":[{"type":"text","text":"If there is no value for the given "},{"type":"codeVoice","code":"key"},{"type":"text","text":", returns the description of this key,"},{"type":"text","text":" "},{"type":"text","text":"i.e.font family name without the font weight in the end and without whitespaces."}],"kind":"symbol","identifier":{"interfaceLanguage":"swift","url":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/Swift\/Dictionary\/subscript(orKey:)"},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/oudsfoundations\/swift\/dictionary\/subscript(orkey:)"]}],"references":{"doc://OU