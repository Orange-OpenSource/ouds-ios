DSFoundations/documentation/OUDSFoundations":{"identifier":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations","title":"OUDSFoundations","type":"topic","url":"\/documentation\/oudsfoundations","abstract":[{"type":"text","text":"Contains a set of helpers shared for higher level targets."}],"role":"collection","kind":"symbol"},"doc://OUDSFoundations/documentation/OUDSFoundations/OUDSWCAG21Ratio/requirementsAAA":{"identifier":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/OUDSWCAG21Ratio\/requirementsAAA","url":"\/documentation\/oudsfoundations\/oudswcag21ratio\/requirementsaaa","abstract":[{"text":"WCAG 2.1 requirement for AAA level needs to have textual component matching 7:1 contrast ratio","type":"text"},{"text":" ","type":"text"},{"text":"and non textual component matching 4.5:1 contrast ratio.","type":"text"},{"text":" ","type":"text"},{"text":"This is recommended for mobiles devices projects (because of screen sizes and luminosity issues).","type":"text"}],"fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"requirementsAAA"},{"kind":"text","text":": "},{"preciseIdentifier":"s:15OUDSFoundations18WCAG21Requirementsa","kind":"typeIdentifier","text":"WCAG21Requirements"}],"type":"topic","kind":"symbol","role":"symbol","title":"requirementsAAA"},"doc://OUDSFoundations/documentation/OUDSFoundations/WCAG21Requirements":{"kind":"symbol","fragments":[{"kind":"keyword","text":"typealias"},{"kind":"text","text":" "},{"kind":"identifier","text":"WCAG21Requirements"}],"title":"WCAG21Requirements","identifier":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/WCAG21Requirements","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"WCAG21Requirements"}],"url":"\/documentation\/oudsfoundations\/wcag21requirements","abstract":[{"text":"WCAG 2.1 AA and AAA requirements use ratio for textual and non textual components","type":"text"}],"type":"topic"},"doc://OUDSFoundations/documentation/OUDSFoundations/OUDSWCAG21Ratio":{"kind":"symbol","url":"\/documentation\/oudsfoundations\/oudswcag21ratio","title":"OUDSWCAG21Ratio","identifier":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/OUDSWCAG21Ratio","abstract":[{"type":"text","text":"Some countries set up new laws so as to force companies to have a better accessibility of their digital services."},{"type":"text","text":" "},{"type":"text","text":"This is for example the case for France with the "},{"type":"reference","identifier":"https:\/\/accessibilite.numerique.gouv.fr","isActive":true},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"also named “RGAA”."},{"type":"text","text":" "},{"type":"text","text":"The WCAG specifications, for AA level, defines a contrast ratio of 4.5:1 for textual elements (i.e. texts), and 3:1 for non-textual elements (i.e. link’s chevron)."},{"type":"text","text":" "},{"type":"text","text":"However it is higly recommended for mobile devices to reach AAA level because of the screen sizes and luminosity issues, i.e. 7:1 for textual"},{"type":"text","text":" "},{"type":"text","text":"and 4.5:1 for non-textual elements."},{"type":"text","text":" "},{"type":"text","text":"This utility helps to define wether a set of contrast ratio matches AA and AAA or not."},{"type":"text","text":" "},{"type":"text","text":"See for example "},{"identifier":"https:\/\/system.design.orange.com\/0c1af118d\/p\/2531b9-accessibility-compliance","type":"reference","isActive":true}],"type":"topic","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OUDSWCAG21Ratio"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"OUDSWCAG21Ratio"}]},"https://accessibilite.numerique.gouv.fr":{"type":"link","identifier":"https:\/\/accessibilite.numerique.gouv.fr","url":"https:\/\/accessibilite.numerique.gouv.fr","title":"Référentiel Général d’Amélioration de l’Accessibilité","titleInlineContent":[{"type":"text","text":"Référenti