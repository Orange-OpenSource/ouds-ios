ample "},{"type":"reference","isActive":true,"identifier":"https:\/\/system.design.orange.com\/0c1af118d\/p\/2531b9-accessibility-compliance"}],"type":"topic","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OUDSWCAG21Ratio","kind":"identifier"}],"navigatorTitle":[{"text":"OUDSWCAG21Ratio","kind":"identifier"}],"url":"\/documentation\/oudsfoundations\/oudswcag21ratio","role":"symbol","kind":"symbol"}}}