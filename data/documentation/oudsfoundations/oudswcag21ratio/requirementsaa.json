nts":[{"kind":"keyword","text":"typealias"},{"kind":"text","text":" "},{"kind":"identifier","text":"WCAG21Requirements"}],"navigatorTitle":[{"kind":"identifier","text":"WCAG21Requirements"}],"type":"topic","title":"WCAG21Requirements","url":"\/documentation\/oudsfoundations\/wcag21requirements","kind":"symbol","abstract":[{"type":"text","text":"WCAG 2.1 AA and AAA requirements use ratio for textual and non textual components"}],"role":"symbol","identifier":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/WCAG21Requirements"},"doc://OUDSFoundations/documentation/OUDSFoundations/OUDSWCAG21Ratio":{"title":"OUDSWCAG21Ratio","type":"topic","url":"\/documentation\/oudsfoundations\/oudswcag21ratio","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OUDSWCAG21Ratio","kind":"identifier"}],"abstract":[{"type":"text","text":"Some countries set up new laws so as to force companies to have a better accessibility of their digital services."},{"type":"text","text":" "},{"type":"text","text":"This is for example the case for France with the "},{"type":"reference","identifier":"https:\/\/accessibilite.numerique.gouv.fr","isActive":true},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"also named “RGAA”."},{"type":"text","text":" "},{"type":"text","text":"The WCAG specifications, for AA level, defines a contrast ratio of 4.5:1 for textual elements (i.e. texts), and 3:1 for non-textual elements (i.e. link’s chevron)."},{"type":"text","text":" "},{"type":"text","text":"However it is higly recommended for mobile devices to reach AAA level because of the screen sizes and luminosity issues, i.e. 7:1 for textual"},{"type":"text","text":" "},{"type":"text","text":"and 4.5:1 for non-textual elements."},{"type":"text","text":" "},{"type":"text","text":"This utility helps to define wether a set of contrast ratio matches AA and AAA or not."},{"type":"text","text":" "},{"type":"text","text":"See for example "},{"type":"reference","identifier":"https:\/\/system.design.orange.com\/0c1af118d\/p\/2531b9-accessibility-compliance","isActive":true}],"identifier":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/OUDSWCAG21Ratio","kind":"symbol","navigatorTitle":[{"text":"OUDSWCAG21Ratio","kind":"identifier"}],"role":"symbol"}}}