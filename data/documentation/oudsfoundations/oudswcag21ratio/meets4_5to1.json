,{"type":"text","text":"This utility helps to define wether a set of contrast ratio matches AA and AAA or not."},{"type":"text","text":" "},{"type":"text","text":"See for example "},{"type":"reference","isActive":true,"identifier":"https:\/\/system.design.orange.com\/0c1af118d\/p\/2531b9-accessibility-compliance"}],"navigatorTitle":[{"kind":"identifier","text":"OUDSWCAG21Ratio"}],"identifier":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/OUDSWCAG21Ratio"},"https://system.design.orange.com/0c1af118d/p/2531b9-accessibility-compliance":{"url":"https:\/\/system.design.orange.com\/0c1af118d\/p\/2531b9-accessibility-compliance","titleInlineContent":[{"text":"the following guidelines","type":"text"}],"type":"link","identifier":"https:\/\/system.design.orange.com\/0c1af118d\/p\/2531b9-accessibility-compliance","title":"the following guidelines"},"https://accessibilite.numerique.gouv.fr":{"url":"https:\/\/accessibilite.numerique.gouv.fr","titleInlineContent":[{"text":"Référentiel Général d’Amélioration de l’Accessibilité","type":"text"}],"type":"link","identifier":"https:\/\/accessibilite.numerique.gouv.fr","title":"Référentiel Général d’Amélioration de l’Accessibilité"}}}