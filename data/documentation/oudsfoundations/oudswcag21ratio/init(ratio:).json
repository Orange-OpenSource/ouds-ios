text"},{"text":" ","type":"text"},{"text":"See for example ","type":"text"},{"isActive":true,"identifier":"https:\/\/system.design.orange.com\/0c1af118d\/p\/2531b9-accessibility-compliance","type":"reference"}],"role":"symbol","kind":"symbol","title":"OUDSWCAG21Ratio","url":"\/documentation\/oudsfoundations\/oudswcag21ratio","type":"topic","navigatorTitle":[{"kind":"identifier","text":"OUDSWCAG21Ratio"}],"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OUDSWCAG21Ratio"}],"identifier":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/OUDSWCAG21Ratio"},"doc://OUDSFoundations/documentation/OUDSFoundations/OUDSWCAG21Ratio/init(ratio:)":{"type":"topic","identifier":"doc:\/\/OUDSFoundations\/documentation\/OUDSFoundations\/OUDSWCAG21Ratio\/init(ratio:)","role":"symbol","title":"init(ratio:)","url":"\/documentation\/oudsfoundations\/oudswcag21ratio\/init(ratio:)","kind":"symbol","fragments":[{"kind":"identifier","text":"init"},{"kind":"text","text":"("},{"kind":"externalParam","text":"ratio"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:Sd","text":"Double"},{"kind":"text","text":")"}],"abstract":[{"type":"text","text":"Defines a WCAG 2.1 ratio with the given contrast ratio value"}]}}}